#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int c, v0, v1, a, l, s = 0, ss = 0;
	cin >> c >> v0 >> v1 >> a >> l;
	while (s < c)
	{
		if (s > 0)
			s -= l;
		int k = v0 + ss * a;
		ss++;
		if (k > v1)
			k = v1;
		s += k;
	}
	cout << ss;
}
