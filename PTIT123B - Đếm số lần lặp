#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, t = 1;
	while (1)
	{
		cin >> n;
		if (n == 0)
			break;
		int a[22];
		for (int i = 1; i <= n; i++)
			cin >> a[i];
		cout << "Case " << t++ << ": ";
		int ok = 1, d = 0;
		for (int k = 1; k <= n; k++)
		{
			if (a[k] != a[1])
			{
				ok = 0;
				break;
			}
		}
		if (ok == 0)
		{
			int ok1 = 0;
			for (int i = 1; i <= 1000; i++)
			{
				d = i;
				int tg = a[1];
				for (int k = 1; k <= n; k++)
				{
					if (k != n)
					{
						if (a[k] > a[k + 1])
							a[k] = a[k] - a[k + 1];
						else
							a[k] = a[k + 1] - a[k];
					}
					else
					{
						if (a[k] > tg)
							a[k] = a[k] - tg;
						else
							a[k] = tg - a[k];
					}
				}
				int ok2 = 1;
				for (int k = 1; k <= n; k++)
				{
					if (a[k] != a[1])
					{
						ok2 = 0;
						break;
					}
				}
				if (ok2 == 1)
				{
					ok1 = 1;
					break;
				}
			}
			if (ok1 == 1)
				cout << d << " iterations" << endl;
			else
				cout << "not attained" << endl;
		}
		else
			cout << "0 iterations" << endl;
	}
}
