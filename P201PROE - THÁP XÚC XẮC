#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        long long n;
        cin >> n;
        if (n <= 14)
            cout << "NO" << endl;
        else if (n % 14 >= 1 && n % 14 <= 6)
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    }
}
