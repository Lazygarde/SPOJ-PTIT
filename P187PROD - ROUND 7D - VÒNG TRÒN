#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
vector<vector<int>> a(26, vector<int>(0));
bool ss(int i, int j)
{
	if (a[i][0] < a[j][0] && a[j][0] < a[i][1] && a[i][1] < a[j][1])
		return 1;
	return 0;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int dem = 0;
	string s;
	cin >> s;
	for (int i = 0; i < 52; i++)
		a[s[i] - 65].push_back(i);
	for (int i = 0; i < 25; i++)
		for (int j = i + 1; j < 26; j++)
			if (ss(i, j) || ss(j, i))
				dem++;
	cout << dem;
}
