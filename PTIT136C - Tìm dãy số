#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n;
    cin >> n;
    int a[n][n], b[n];
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> a[i][j];
    if (n == 2)
    {
        cout << "1 1";
        return 0;
    }
    b[0] = (a[1][0] + a[2][0] - a[2][1]) / 2;
    for (int i = 1; i < n; i++)
        b[i] = a[0][i] - b[0];
    for (int i = 0; i < n; i++)
        cout << b[i] << " ";
}
