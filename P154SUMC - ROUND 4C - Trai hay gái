#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string s;
	cin >> s;
	int a[200] = {}, ss = 0;
	for (int i = 0; i < s.size(); i++)
		a[s[i]]++;
	for (int i = 'a'; i <= 'z'; i++)
		if (a[i] > 0)
			ss++;
	if (ss % 2 == 0)
		cout << "CHAT WITH HER!";
	else
		cout << "IGNORE HIM!";
}
