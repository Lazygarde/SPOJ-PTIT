#include <bits/stdc++.h>
using namespace std;
long long chiadu(string a,long long b){
	long long du=0;
    for (int i=0;i<a.length();i++)
        du=(du*10+a[i]-48)%b;
    return du;
}
long long pow1(long long n,long long m,long long p){
	if(m==1) return n;
	if(m%2==0) return (pow1(n,m/2,p)*pow1(n,m/2,p))%p;
	else return (n*pow1(n,m-1,p))%p;
}
int main(){
	int t;
	cin>>t;
	while(t--){
		string a;
		long long b,m;
		cin>>a>>b>>m;
		long long k=chiadu(a,m);
		cout<<pow1(k,b,m)<<endl;
	}
}
