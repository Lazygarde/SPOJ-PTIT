#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, m, maxx = 0;
    cin >> n >> m;
    int a[n];
    for (int i = 0; i < n; i++)
        cin >> a[i];
    for (int i1 = 0; i1 < n - 2; i1++)
        for (int i2 = i1 + 1; i2 < n - 1; i2++)
            for (int i3 = i2 + 1; i3 < n; i3++)
                if (a[i1] + a[i2] + a[i3] > maxx && a[i1] + a[i2] + a[i3] <= m)
                    maxx = a[i1] + a[i2] + a[i3];
    cout << maxx;
}
