#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n;
		cin >> n;
		int a[n + 1], s = 0;
		for (int i = 1; i <= n; i++)
			a[i] = 1;
		for (int i = 2; i <= n; i++)
		{
			for (int j = i; j <= n; j++)
			{
				if (j % i == 0)
				{
					if (a[j] == 1)
						a[j] = 0;
					else
						a[j] = 1;
				}
			}
		}
		for (int i = 1; i <= n; i++)
			if (a[i] == 1)
				s++;
		cout << s << endl;
	}
}
