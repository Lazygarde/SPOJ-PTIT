#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long r, h, d;
	cin >> r >> h;
	d = 2 * (h / r);
	h %= r;
	double rr = (double)r / 2;
	if (h >= rr + rr * sqrt(3) / 2)
		d += 3;
	else if (h >= rr)
		d += 2;
	else
		d++;
	cout << d;
}
