#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int s = 0;
        double a, b, c;
        cin >> a >> b >> c;
        while (a < c)
        {
            s++;
            a = a * (1 + b / 100);
        }
        cout << s << endl;
    }
}
