#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, ok = 1;
	string s;
	cin >> n >> s;
	int a[125] = {};
	for (int i = 0; i < n; i++)
	{
		if (s[i] >= 97 && s[i] <= 122)
			s[i] -= 32;
		a[s[i]]++;
	}
	for (int i = 65; i <= 90; i++)
	{
		if (a[i] == 0)
		{
			ok = 0;
			break;
		}
	}
	if (ok)
		cout << "YES";
	else
		cout << "NO";
}
