#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int x, y;
	int t[] = {0, 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
	cin >> x >> y;
	int s;
	if (y != 1)
	{
		s = 1;
		t[x] = t[x] - (8 - y);
		s += t[x] / 7;
		if (t[x] % 7 != 0)
			s++;
	}
	else
	{
		s = t[x] / 7;
		if (t[x] % 7 != 0)
			s++;
	}
	cout << s;
}
