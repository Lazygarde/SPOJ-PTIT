#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    string a, b;
    cin >> a >> b;
    int c[10] = {};
    for (int i = 0; i < b.size(); i++)
        c[b[i] - '0']++;
    for (int i = 0; i < a.size(); i++)
    {
        for (int j = 9; j > a[i] - '0'; j--)
        {
            if (c[j] > 0)
            {
                c[j]--;
                a[i] = j + '0';
                break;
            }
        }
    }
    cout << a;
}
