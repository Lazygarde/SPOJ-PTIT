#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string s;
	cin >> s;
	stack<int> st;
	for (int i = 0; i < s.length(); i++)
	{
		if (s[i] == '(')
			st.push(0);
		else if (s[i] == ')')
		{
			int k = 0;
			while (!st.empty() && st.top() != 0)
			{
				k += st.top();
				st.pop();
			}
			if (!st.empty() && st.top() == 0)
			{
				st.pop();
				st.push(k);
			}
		}
		else if (s[i] >= '0' && s[i] <= '9')
		{
			int so = s[i] - '0';
			if (!st.empty())
			{
				int k = st.top();
				st.pop();
				k = k * so;
				st.push(k);
			}
		}
		else if (s[i] == 'C')
			st.push(12);
		else if (s[i] == 'H')
			st.push(1);
		else if (s[i] == 'O')
			st.push(16);
	}
	int S = 0;
	while (!st.empty())
	{
		S += st.top();
		st.pop();
	}
	cout << S;
}
