#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
bool check(string a)
{
	int n = a.size();
	for (int i = 0; i <= n / 2; i++)
		if (a[i] != a[n - i - 1])
			return 0;
	return 1;
}
string Find(string s)
{
	int n = s.size();
	string ans = s;
	while (1)
	{
		if (check(ans))
			return ans;
		for (int i = 0, j = n - 1; i < n, j >= 0; i++, j--)
		{
			if (i >= j)
				break;
			if (ans[i] >= ans[j])
				ans[j] = ans[i];
			else
			{
				ans[j] = ans[i];
				int tmp = (ans[j - 1] + 1 - '0') / 10;
				ans[j - 1]++;
				if (ans[j - 1] - '0' >= 10)
				{
					tmp = 1;
					ans[j - 1] -= 10;
				}
				else
					tmp = 0;
				if (tmp != 0)
				{
					int k = j - 1;
					while (tmp != 0)
					{
						ans[k - 1]++;
						if (ans[k - 1] - '0' >= 10)
						{
							tmp = 1;
							ans[k - 1] -= 10;
						}
						else
							tmp = 0;
						k--;
					}
				}
			}
		}
	}
	return ans;
}

int trans(string s)
{
	int n = 0;
	for (int i = 0; i < s.size(); i++)
		n = n * 10 + s[i] - '0';
	return n;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	while (1)
	{
		string s;
		cin >> s;
		if (s == "0")
			break;
		cout << trans(Find(s)) - trans(s) << endl;
	}
}
