#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		long long x1, x2, x3, a[3];
		cin >> x1 >> x2 >> x3;
		a[0] = abs(x1 - x2);
		a[1] = abs(x1 - x3);
		a[2] = abs(x2 - x3);
		sort(a, a + 3);
		cout << a[0] + a[1] << endl;
	}
}
