#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	char a[n], b[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	for (int i = 0; i < n; i++)
		cin >> b[i];
	for (int i = 0; i < n; i++)
	{
		if (a[i] == b[i])
		{
			if (a[i] == 'K')
				cout << 'B';
			else if (a[i] == 'B')
				cout << 'G';
			else
				cout << 'K';
		}
		else
		{
			if (a[i] == 'K' && b[i] == 'B')
				cout << 'B';
			else if (a[i] == 'B' && b[i] == 'K')
				cout << 'B';
			else if (a[i] == 'K' && b[i] == 'G')
				cout << 'K';
			else if (a[i] == 'G' && b[i] == 'K')
				cout << 'K';
			else if (a[i] == 'B' && b[i] == 'G')
				cout << 'G';
			else if (a[i] == 'G' && b[i] == 'B')
				cout << 'G';
		}
	}
}
