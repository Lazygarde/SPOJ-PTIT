#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, k;
	char c;
	cin >> n >> k;
	vector<int> a;
	for (int i = 1; i <= n; i++)
	{
		cin >> c;
		int d = c - '0';
		if (a.empty())
			a.push_back(d);
		else
		{
			while (c > a.back() + '0' && k > 0 && !a.empty())
			{
				a.pop_back();
				k--;
			}
			a.push_back(d);
		}
	}
	while (k > 0 && !a.empty())
	{
		a.pop_back();
		k--;
	}
	for (auto i : a)
		cout << i;
}
