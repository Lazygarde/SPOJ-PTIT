#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int m, n, s = 0;
	cin >> m >> n;
	for (int i = 0; i <= 1000; i++)
	{
		for (int j = 0; j <= 1000; j++)
		{
			if (i * i + j == m && i + j * j == n)
				s++;
		}
	}
	cout << s;
}
