#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int d = 1, i = 2, x;
	cin >> x;
	while (x >= d)
	{
		x -= d;
		d += i;
		i++;
	}
	cout << i - 2;
}
