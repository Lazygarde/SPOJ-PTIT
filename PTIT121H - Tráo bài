#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, k;
	cin >> n >> k;
	string a;
	cin >> a;
	deque<int> b;
	for (int i = 0; i < n; i++)
		b.push_back(i);
	for (int i = 0; i < a.size() - 1; i++)
	{
		if (a[i] == 'A')
		{
			b.push_back(b.front());
			b.pop_front();
		}
		else
		{
			int h = b.front();
			b.pop_front();
			b.push_back(b.front());
			b.pop_front();
			b.push_front(h);
		}
	}
	for (int i = 0; i < k - 1; i++)
		b.pop_front();
	for (int i = 0; i < 3; i++)
	{
		cout << b.front() << " ";
		b.pop_front();
	}
}
