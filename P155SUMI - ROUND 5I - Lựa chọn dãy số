#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, m;
	cin >> n >> m;
	int a[m];
	for (int i = 0; i < m; i++)
		cin >> a[i];
	sort(a, a + m);
	int s = 2000;
	for (int i = 0; i < m - n + 1; i++)
		if (a[i + n - 1] - a[i] < s)
			s = a[i + n - 1] - a[i];
	cout << s;
}
