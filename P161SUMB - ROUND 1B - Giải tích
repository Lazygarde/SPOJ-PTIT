#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int m, n;
	cin >> m >> n;
	int a[m + 1], b[n + 1];
	for (int i = 0; i <= m; i++)
		cin >> a[i];
	for (int i = 0; i <= n; i++)
		cin >> b[i];
	if (m < n)
		cout << "0/1";
	else if (m == n)
	{
		if (a[0] > 0)
		{
			if (b[0] > 0)
			{
				int k = __gcd(a[0], b[0]);
				cout << a[0] / k << "/" << b[0] / k;
			}
			else
			{
				b[0] *= -1;
				int k = __gcd(a[0], b[0]);
				cout << "-" << a[0] / k << "/" << b[0] / k;
			}
		}
		else
		{
			if (b[0] < 0)
			{
				a[0] *= -1;
				b[0] *= -1;
				int k = __gcd(a[0], b[0]);
				cout << a[0] / k << "/" << b[0] / k;
			}
			else
			{
				a[0] *= -1;
				int k = __gcd(a[0], b[0]);
				cout << "-" << a[0] / k << "/" << b[0] / k;
			}
		}
	}
	else
	{
		if (a[0] * b[0] > 0)
			cout << "Infinity";
		else
			cout << "-Infinity";
	}
}
