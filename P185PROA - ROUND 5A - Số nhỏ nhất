#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		string a;
		cin >> a;
		int j = 0;
		if (a[0] == '9')
			j++;
		for (int i = j; i < a.size(); i++)
			if (a[i] > '4')
				a[i] = '9' - a[i] + '0';
		cout << a << endl;
	}
}
