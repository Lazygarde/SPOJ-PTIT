#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
vector<bool> check;
vector<vector<int>> a;
void dfs(int u) {
	check[u] = 0;
	for (auto v : a[u]) {
		if (check[v]) {
			dfs(v);
		}
	}
}
int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, m, x, y, stp = 0, s;
	cin >> n >> m;
	a.clear();
	a.resize(n + 1);
	check.clear();
	check.resize(n + 1, 1);
	for (int i = 0; i < m; i++) {
		cin >> x >> y;
		a[x].push_back(y);
		a[y].push_back(x);
	}
	for (int i = 1; i <= n; i++) {
		if (check[i]) {
			stp++;
			check[i] = 0;
			dfs(i);
		}
	}
	for (int i = 1; i <= n; i++) {
		check.clear();
		check.resize(n + 1, 1);
		s = 0;
		check[i] = 0;
		for (auto j : a[i]) {
			if (check[j]) {
				s++;
				check[j] = 0;
				dfs(j);
			}
		}
		cout << stp + s - 1 << endl;
	}
}
