#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, k, s = 0, d = 0;
	cin >> n >> k;
	long long a[n];
	vector<long long> b(n);
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
		b[i] = a[i];
		if (a[i] > s)
			s = a[i];
	}
	if (k >= n - 1)
		cout << s;
	else
	{
		while (1)
		{
			if (b[0] < b[1])
			{
				d = 1;
				b.push_back(b[0]);
				b.erase(b.begin(), b.begin() + 1);
			}
			else
			{
				d++;
				if (d == k)
				{
					cout << b[0];
					return 0;
				}
				b.push_back(b[1]);
				b.erase(b.begin() + 1, b.begin() + 2);
			}
		}
	}
}
