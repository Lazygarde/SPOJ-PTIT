#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    while (1)
    {
        int n;
        cin >> n;
        if (n == 0)
            return 0;
        int a[n + 1];
        for (int i = 1; i <= n; i++)
            cin >> a[i];
        int x = 1;
        for (int i = 1; i <= n; i++)
        {
            for (int j = x; j <= a[i]; j++)
            {
                cout << i << " ";
                x++;
            }
        }
        cout << endl;
    }
}
