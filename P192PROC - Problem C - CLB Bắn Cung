#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	long long a[n], s = 0;
	for (int i = 0; i < n; i++)
		cin >> a[i];
	long long b[n + 1] = {};
	b[n] = 0;
	for (int i = n - 1; i >= 0; i--)
		b[i] = a[i] + b[i + 1];
	for (int i = 0; i < n; i++)
		s += a[i] * b[i + 1];
	cout << s;
}
