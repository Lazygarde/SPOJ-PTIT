#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int m, n;
	cin >> n >> m;
	char f[n][m];
	for (int i = 0; i < n; i++)
		for (int j = 0; j < m; j++)
			cin >> f[i][j];
	int a, b, c, d, ok = 1;
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
		{
			if (f[i][j] == 'B')
			{
				a = i;
				b = j;
				ok = 0;
				break;
			}
		}
		if (ok == 0)
			break;
	}
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
		{
			if (f[i][j] == 'B')
			{
				c = i;
				d = j;
			}
		}
	}
	cout << (a + c) / 2 + 1 << " " << (b + d) / 2 + 1;
}
