#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		string s1, s2, s3;
		cin >> s1 >> s2 >> s3;
		int a[12] = {};
		for (int j = 0; j < s1.size(); j++)
		{
			int so = s1[j] - '0';
			a[so]++;
		}
		for (int j = 0; j < s2.size(); j++)
		{
			int so = s2[j] - '0';
			a[so]++;
		}
		for (int j = 0; j < s3.size(); j++)
		{
			int so = s3[j] - '0';
			a[so]++;
		}
		for (int j = 9; j >= 0; j--)
			for (int k = 0; k < a[j]; k++)
				cout << j;
		cout << endl;
	}
}
