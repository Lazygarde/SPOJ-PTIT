#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n, s = 0;
		cin >> n;
		int a[n];
		for (int i = 0; i < n; i++)
			cin >> a[i];
		sort(a, a + n);
		int j = 1;
		while (j < n)
		{
			if (a[j] == a[j - 1])
			{
				s++;
				j += 2;
			}
			else
				j++;
		}
		cout << s / 2 << endl;
	}
}
