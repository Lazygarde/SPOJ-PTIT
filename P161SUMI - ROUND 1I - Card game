#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, k, b[26] = {}, s = 0, ss = 0, pos = 25;
	cin >> n >> k;
	char a[n];
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
		b[a[i] - 'A']++;
	}
	sort(b, b + 26);
	while (1)
	{
		if (b[pos] + ss >= k)
		{
			s += (k - ss) * (k - ss);
			break;
		}
		else
		{
			ss += b[pos];
			s += b[pos] * b[pos];
			pos--;
		}
	}
	cout << s;
}
