#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int a, b, c, d[3][2], e[101] = {}, s = 0;
    cin >> a >> b >> c;
    for (int i = 0; i < 3; i++)
        cin >> d[i][0] >> d[i][1];
    for (int i = 0; i < 3; i++)
        for (int j = d[i][0]; j < d[i][1]; j++)
            e[j]++;
    for (int i = 0; i < 101; i++)
    {
        if (e[i] == 3)
            s += 3 * c;
        else if (e[i] == 2)
            s += 2 * b;
        else if (e[i] == 1)
            s += a;
    }
    cout << s;
}
