#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n;
	cin >> n;
	vector<bool> a(1e9, false);
	for (int i = 1; i <= 44720; i++)
	{
		long long k = (i + 1) * i / 2;
		a[k] = true;
	}
	for (int i = 1; i <= 44720; i++)
	{
		long long k = (i + 1) * i / 2;
		if (k > n)
			break;
		if (a[n - k] == true)
		{
			cout << "YES";
			return 0;
		}
	}
	cout << "NO";
}
