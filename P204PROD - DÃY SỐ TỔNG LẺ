#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n, s1 = 0, s2 = 0;
		cin >> n;
		int a[n];
		for (int i = 0; i < n; i++)
		{
			cin >> a[i];
			if (a[i] % 2 == 0)
				s1++;
			else
				s2++;
		}
		if (n % 2 == 1 && s2 == n)
			cout << "YES\n";
		else if (s1 > 0 && s2 > 0)
			cout << "YES\n";
		else
			cout << "NO\n";
	}
}
