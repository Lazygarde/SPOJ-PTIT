#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	while (1)
	{
		string a;
		getline(cin, a);
		if (a == ".")
			break;
		vector<char> b;
		int ok = 1;
		for (int i = 0; i < a.size(); i++)
		{
			if (a[i] == '(' || a[i] == '[')
				b.push_back(a[i]);
			else if (a[i] == ')' || a[i] == ']')
			{
				if (b.size() == 0)
				{
					ok = 0;
					break;
				}
				if (a[i] == ')' && b[b.size() - 1] == '(' || a[i] == ']' && b[b.size() - 1] == '[')
					b.erase(b.end() - 1);
				else
				{
					ok = 0;
					break;
				}
			}
		}
		if (b.size() != 0)
			ok = 0;
		if (ok == 1)
			cout << "yes\n";
		else
			cout << "no\n";
	}
}
