#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
vector<int> a(246915, 1), b, pos(246915);
void era()
{
	for (long long i = 2; i <= 246913; i++)
	{
		if (a[i] == 1)
		{
			b.push_back(i);
			for (long long j = i * i; j <= 246913; j += i)
				a[j] = 0;
		}
		pos[i] = b.size() - 1;
	}
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	era();
	while (1)
	{
		int n, s = 0;
		cin >> n;
		if (n == 0)
			break;
		for (int i = pos[n]; i <= pos[2 * n]; i++)
		{
			if (b[i] > n && b[i] <= 2 * n)
				s++;
		}
		cout << s << endl;
	}
}
