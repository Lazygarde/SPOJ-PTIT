#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n;
    cin >> n;
    int a[n], b[n], s = 0;
    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
    }
    for (int i = 0; i < n; i++)
    {
        cin >> b[i];
    }
    sort(a, a + n);
    sort(b, b + n);
    int i = 0, j = 0;
    while (1)
    {
        if (b[i] > a[j])
        {
            s++;
            i++;
            j++;
        }
        else
            i++;
        if (i == n || j == n)
            break;
    }
    cout << s;
}
