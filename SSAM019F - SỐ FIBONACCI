#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n;
        cin >> n;
        if (n < 3)
            cout << "1\n";
        else
        {
            long long c = 1, d = 1;
            for (int i = 0; i < n - 2; i++)
            {
                long long m = d;
                d += c;
                c = m;
            }
            cout << d << "\n";
        }
    }
}
