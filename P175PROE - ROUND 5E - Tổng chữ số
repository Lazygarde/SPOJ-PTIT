#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string a;
	cin >> a;
	int ans = 0;
	while (a.size() > 1)
	{
		int s = 0;
		for (int i = 0; i < a.size(); i++)
			s += a[i] - '0';
		a = "";
		while (s != 0)
		{
			char x = s % 10 + '0';
			a = x + a;
			s /= 10;
		}
		ans++;
	}
	cout << ans;
}
