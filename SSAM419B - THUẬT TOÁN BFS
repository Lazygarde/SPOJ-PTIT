#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n, m, u, x, y;
        cin >> n >> m >> u;
        queue<int> q;
        vector<bool> check(n + 1, 1);
        vector<vector<int>> a(n + 1);
        for (int i = 0; i < m; i++)
        {
            cin >> x >> y;
            a[x].push_back(y);
            a[y].push_back(x);
        }
        for (auto i : a)
            sort(i.begin(), i.end());
        q.push(u);
        check[u] = 0;
        while (q.size())
        {
            u = q.front();
            q.pop();
            cout << u << " ";
            for (auto i : a[u])
            {
                if (check[i])
                {
                    check[i] = 0;
                    q.push(i);
                }
            }
        }
        cout << endl;
    }
}
