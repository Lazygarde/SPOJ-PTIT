#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, m;
    cin >> m >> n;
    char a[600][600];
    for (int i = 0; i < 5 * m + 1; i++)
        for (int j = 0; j < 5 * n + 1; j++)
            cin >> a[i][j];
    int b[] = {0, 0, 0, 0, 0};
    int s = 0, c = 1, d = 1;
    for (int i = 0; i < m; i++)
    {
        if (i != 0)
            c += 5;
        d = 1;
        for (int j = 0; j < n; j++)
        {
            if (j != 0)
                d += 5;
            b[0]++;
            for (int k = 0; k < 4; k++)
            {
                if (a[c + k][d] == '*')
                {
                    b[k + 1]++;
                    b[k]--;
                }
            }
        }
    }
    for (int i = 0; i < 5; i++)
        cout << b[i] << " ";
}
