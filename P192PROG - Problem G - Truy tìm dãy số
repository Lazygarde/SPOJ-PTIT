#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n;
    cin >> n;
    long long a[n], b[n / 2];
    for (int i = 0; i < n / 2; i++)
        cin >> b[i];
    a[0] = 0;
    a[n - 1] = b[0];
    for (int i = 1; i < n / 2; i++)
    {
        a[i] = a[i - 1];
        if (b[i] > b[i - 1])
            a[i] += b[i] - b[i - 1];
        a[n - i - 1] = b[i] - a[i];
    }
    for (int i = 0; i < n; i++)
        cout << a[i] << " ";
}
