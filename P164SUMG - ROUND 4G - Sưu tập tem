#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string a;
	cin >> a;
	vector<string> b;
	int s = 0;
	for (int i = 0; i <= a.size(); i++)
	{
		for (int j = 0; j < 26; j++)
		{
			string c = a;
			char k = j + 'a';
			c.insert(i, 1, k);
			b.push_back(c);
		}
	}
	sort(b.begin(), b.end());
	for (int i = 1; i < b.size(); i++)
	{
		if (b[i] == b[i - 1])
			s++;
	}
	cout << b.size() - s;
}
