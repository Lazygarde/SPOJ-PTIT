#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, k, h = 0;
	string s;
	cin >> n >> k;
	cin >> s;
	vector<bool> a(n / 2, 0);
	for (int i = 0; i < n / 2; i++)
	{
		if (s[i] != s[n - 1 - i])
		{
			h++;
			a[i] = 1;
		}
	}
	if (k < h)
	{
		cout << -1;
		return 0;
	}
	for (int i = 0; i < n / 2; i++)
	{
		if (s[i] != s[n - 1 - i])
		{
			s[i] = max(s[i], s[n - 1 - i]);
			s[n - 1 - i] = s[i];
			k--;
		}
	}
	if (k > 0)
	{
		for (int i = 0; i < n / 2; i++)
		{
			if (k <= 0)
				break;
			if (s[i] != '9')
			{
				if (a[i] == false)
				{
					if (k > 1)
					{
						s[i] = '9';
						s[n - 1 - i] = '9';
						k -= 2;
					}
				}
				else
				{
					s[i] = '9';
					s[n - 1 - i] = '9';
					k--;
				}
			}
		}
	}
	if (k > 0 && n % 2 == 1)
		s[(int)n / 2] = '9';
	cout << s;
}
