#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int n, k, ans, a[101];
void Try(int pos, int s)
{
    if (s == k)
    {
        ans++;
        return;
    }
    for (int i = pos + 1; i <= n; i++)
    {
        if (a[i] > a[pos])
        {
            Try(i, s + 1);
        }
    }
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        ans = 0;
        a[0] = -1;
        cin >> n >> k;
        for (int i = 1; i <= n; i++)
            cin >> a[i];
        Try(0, 0);
        cout << ans << endl;
    }
}
