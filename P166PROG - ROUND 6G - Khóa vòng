#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, s = 0;
	cin >> n;
	string a, b;
	cin >> a >> b;
	for (int i = 0; i < n; i++)
	{
		int c = a[i], d = b[i];
		if (c < d)
			swap(c, d);
		if (c - d > d + 10 - c)
			s += d + 10 - c;
		else
			s += c - d;
	}
	cout << s;
}
