#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, m, s = 0, L, R, k;
	cin >> n >> m;
	if (m <= 1e6 && n <= 1e3)
	{
		L = m;
		R = m + 4e4;
	}
	else
	{
		L = m;
		R = m + 4e5;
	}
	bool a[R - L + 1] = {};
	for (long long i = 2; i * i <= R; ++i)
	{
		if (i * i > (L + i - 1) / i * i)
			k = i * i;
		else
			k = (L + i - 1) / i * i;
		for (long long j = k; j <= R; j += i)
			a[j - L] = true;
	}
	if (1 >= L)
		a[1 - L] = true;
	for (int i = L; i < R; i++)
	{
		if (a[i - L] == false)
		{
			cout << i << " ";
			s++;
			if (s == n)
				break;
		}
	}
}
