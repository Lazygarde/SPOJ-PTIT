#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, k = 0;
	cin >> n;
	string a[n], s, s0[2];
	s0[0] = "";
	s0[1] = "";
	cin >> s;
	for (int i = 0; i < n; i++)
		cin >> a[i];
	for (int i = 0; i < s.size(); i++)
	{
		if (s[i] == '*')
			k++;
		else
			s0[k] = s0[k] + s[i];
	}
	int m1 = s0[0].size(), m2 = s0[1].size();
	for (int i = 0; i < n; i++)
	{
		int m = a[i].size(), ok = 0;
		for (int j = 0; j < m; j++)
		{
			string s3 = "";
			for (int h = j; h < j + m1; h++)
				s3 = s3 + a[i][h];
			if (s3 == s0[0])
			{
				for (int g = j + m1; g < m; g++)
				{
					string s4 = "";
					for (int f = g; f < g + m2; f++)
						s4 = s4 + a[i][f];
					if (s4 == s0[1])
					{
						ok = 1;
						break;
					}
				}
			}
		}
		if (ok == 1)
			cout << "DA\n";
		else
			cout << "NE\n";
	}
}
