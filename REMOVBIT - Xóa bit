#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    string s;
    cin >> s;
    string ss = s, max = ss.erase(0, 1);
    for (int i = 1; i < s.size(); i++)
    {
        ss = s;
        if (ss.erase(i, 1) > max)
            max = ss;
    }
    cout << max;
}
