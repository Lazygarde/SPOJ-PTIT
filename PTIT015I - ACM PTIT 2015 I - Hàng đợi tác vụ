#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n, k, s = 0, p = 0, ok = 1;
		cin >> n >> k;
		int a[n];
		for (int i = 0; i < n; i++)
			cin >> a[i];
		for (int i = 0; i < n; i++)
		{
			s += a[i];
			if (s > k)
			{
				p = i;
				ok = 0;
				break;
			}
		}
		if (p == 0 && ok == 1)
			cout << n << endl;
		else
			cout << p << endl;
	}
}
