#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, m, k;
	cin >> n >> m >> k;
	int a[n], b[m], c[k], s1 = 0, s2 = 0, s3 = 0, pos1 = 0, pos2 = 0, pos3 = 0;
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
		s1 += a[i];
	}
	for (int i = 0; i < m; i++)
	{
		cin >> b[i];
		s2 += b[i];
	}
	for (int i = 0; i < k; i++)
	{
		cin >> c[i];
		s3 += c[i];
	}
	while (1)
	{
		if (s1 == s2 && s2 == s3)
			break;
		int s = min(s1, min(s2, s3));
		while (s1 > s)
		{
			s1 -= a[pos1];
			pos1++;
		}
		while (s2 > s)
		{
			s2 -= b[pos2];
			pos2++;
		}
		while (s3 > s)
		{
			s3 -= c[pos3];
			pos3++;
		}
	}
	cout << s1;
}
