#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int a, b;
    cin >> a >> b;
    int c[a + 1], d[b + 1];
    for (int i = 0; i <= a; i++)
        cin >> c[i];
    for (int i = 0; i <= b; i++)
        cin >> d[i];
    if (a > b)
    {
        if ((float)c[0] / d[0] < 0)
            cout << "-Infinity";
        else
            cout << "Infinity";
    }
    else if (b > a)
        cout << "0/1";
    else
    {
        if ((float)c[0] / d[0] < 0)
        {
            if (c[0] < 0)
                c[0] *= -1;
            if (d[0] < 0)
                d[0] *= -1;
            int k = __gcd(c[0], d[0]);
            cout << "-" << c[0] / k << "/" << d[0] / k;
        }
        else
        {
            if (c[0] < 0)
                c[0] *= -1;
            if (d[0] < 0)
                d[0] *= -1;
            int k = __gcd(c[0], d[0]);
            cout << c[0] / k << "/" << d[0] / k;
        }
    }
}
