#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
bool cmp(long long a[], int l, int r, long long s)
{
    while (l <= r)
    {
        long long h = (l + r) / 2;
        if (a[h] == s)
            return true;
        if (a[h] < s)
            l = h + 1;
        else
            r = h - 1;
    }
    return false;
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        long long n, k, s = 0, ok = 0;
        cin >> n >> k;
        long long a[n + 1], b[n + 1];
        b[0] = 0;
        for (int i = 1; i <= n; i++)
        {
            cin >> a[i];
            b[i] = b[i - 1] + a[i];
        }
        for (int i = 1; i <= n; i++)
        {
            if (b[i] == k)
            {
                cout << "YES" << endl;
                ok = 1;
                break;
            }
            if (b[i] > k)
            {
                if (cmp(b, 0, i - 1, b[i] - k))
                {
                    cout << "YES" << endl;
                    ok = 1;
                    break;
                }
            }
        }
        if (ok == 0)
            cout << "NO" << endl;
    }
}
