#include <iostream>
using namespace std;
int main(){
	while(1){
		string a;
		cin>>a;
		if(a=="#") return 0;
		for(int i=0;i<a.length();i++){
			if(a[i]>='a'&&a[i]<='c'){
				a[i]-=(i+1);
				if(a[i]>'c'){
					while(a[i]>'c') a[i]-=3;
				}
				if(a[i]<'a'){
					while(a[i]<'a') a[i]+=3;
				}
			}
			if(a[i]>='d'&&a[i]<='f'){
				a[i]-=(i+1);
				if(a[i]>'f'){
					while(a[i]>'f') a[i]-=3;
				}
				if(a[i]<'d'){
					while(a[i]<'d') a[i]+=3;
				}
			}
			if(a[i]>='g'&&a[i]<='i'){
				a[i]-=(i+1);
				if(a[i]>'i'){
					while(a[i]>'i') a[i]-=3;
				}
				if(a[i]<'g'){
					while(a[i]<'g') a[i]+=3;
				}
			}
			if(a[i]>='j'&&a[i]<='l'){
				a[i]-=(i+1);
				if(a[i]>'l'){
					while(a[i]>'l') a[i]-=3;
				}
				if(a[i]<'j'){
					while(a[i]<'j') a[i]+=3;
				}
			}
			if(a[i]>='m'&&a[i]<='o'){
				a[i]-=(i+1);
				if(a[i]>'o'){
					while(a[i]>'o') a[i]-=3;
				}
				if(a[i]<'m'){
					while(a[i]<'m') a[i]+=3;
				}
			}
			if(a[i]>='p'&&a[i]<='s'){
				a[i]-=(i+1);
				if(a[i]>'s'){
					while(a[i]>'s') a[i]-=4;
				}
				if(a[i]<'p'){
					while(a[i]<'p') a[i]+=4;
				}
			}
			if(a[i]>='t'&&a[i]<='v'){
				a[i]-=(i+1);
				if(a[i]>'v'){
					while(a[i]>'v') a[i]-=3;
				}
				if(a[i]<'t'){
					while(a[i]<'t') a[i]+=3;
				}
			}
			if(a[i]>='w'&&a[i]<='z'){
				a[i]-=(i+1);
				if(a[i]>'z'){
					while(a[i]>'z') a[i]-=4;
				}
				if(a[i]<'w'){
					while(a[i]<'w') a[i]+=4;
				}
			}
			if(a[i]>='A'&&a[i]<='C'){
				a[i]-=(i+1);
				if(a[i]>'C'){
					while(a[i]>'C') a[i]-=3;
				}
				if(a[i]<'A'){
					while(a[i]<'A') a[i]+=3;
				}
			}
			if(a[i]>='D'&&a[i]<='F'){
				a[i]-=(i+1);
				if(a[i]>'F'){
					while(a[i]>'F') a[i]-=3;
				}
				if(a[i]<'D'){
					while(a[i]<'D') a[i]+=3;
				}
			}
			if(a[i]>='G'&&a[i]<='I'){
				a[i]-=(i+1);
				if(a[i]>'I'){
					while(a[i]>'I') a[i]-=3;
				}
				if(a[i]<'G'){
					while(a[i]<'G') a[i]+=3;
				}
			}
			if(a[i]>='J'&&a[i]<='L'){
				a[i]-=(i+1);
				if(a[i]>'L'){
					while(a[i]>'L') a[i]-=3;
				}
				if(a[i]<'J'){
					while(a[i]<'J') a[i]+=3;
				}
			}
			if(a[i]>='M'&&a[i]<='O'){
				a[i]-=(i+1);
				if(a[i]>'O'){
					while(a[i]>'O') a[i]-=3;
				}
				if(a[i]<'M'){
					while(a[i]<'M') a[i]+=3;
				}
			}
			if(a[i]>='P'&&a[i]<='S'){
				a[i]-=(i+1);
				if(a[i]>'S'){
					while(a[i]>'S') a[i]-=4;
				}
				if(a[i]<'P'){
					while(a[i]<'P') a[i]+=4;
				}
			}
			if(a[i]>='T'&&a[i]<='V'){
				a[i]-=(i+1);
				if(a[i]>'V'){
					while(a[i]>'V') a[i]-=3;
				}
				if(a[i]<'T'){
					while(a[i]<'T') a[i]+=3;
				}
			}
			if(a[i]>='W'&&a[i]<='Z'){
				a[i]-=(i+1);
				if(a[i]>'Z'){
					while(a[i]>'Z') a[i]-=4;
				}
				if(a[i]<'W'){
					while(a[i]<'W') a[i]+=4;
				}
			}
		}
		for(int i=0;i<a.length();i++){
			cout<<a[i];
		}
		cout<<endl;
	}
}
