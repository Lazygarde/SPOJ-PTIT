#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
bool check1(char a, char b)
{
	if (a == 'c' && b == '=')
		return true;
	if (a == 'c' && b == '-')
		return true;
	if (a == 'd' && b == '-')
		return true;
	if (a == 'l' && b == 'j')
		return true;
	if (a == 'n' && b == 'j')
		return true;
	if (a == 's' && b == '=')
		return true;
	if (a == 'z' && b == '=')
		return true;
	return false;
}
bool check2(char a, char b, char c)
{
	if (a == 'd' && b == 'z' && c == '=')
		return true;
	return false;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string s;
	cin >> s;
	s += "   ";
	int i = 0, ans = 0;
	while (i < s.length())
	{
		if (check2(s[i], s[i + 1], s[i + 2]))
		{
			ans++;
			i += 3;
		}
		else if (check1(s[i], s[i + 1]))
		{
			ans++;
			i += 2;
		}
		else
		{
			ans++;
			i++;
		}
	}
	cout << ans - 3;
}
