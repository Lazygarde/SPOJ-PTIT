#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int sodacbiet(long long k)
{
	for (long long j = 2; j <= sqrt(k); j++)
		if (k % j == 0)
			if (sqrt(j) == (float)sqrt(j) || sqrt(k / j) == (float)sqrt(k / j))
				return 0;
	if (sqrt(k) == (float)sqrt(k))
		return 0;
	return 1;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, s = 0;
	cin >> n;
	if (n == 1)
	{
		cout << "1";
		return 0;
	}
	for (long long i = 1; i <= sqrt(n); i++)
	{
		if (n % i == 0)
		{
			if (sodacbiet(i) == 1 && s < i)
				s = i;
			if (sodacbiet(n / i) == 1 && s < n / i)
				s = n / i;
		}
	}
	cout << s;
}
