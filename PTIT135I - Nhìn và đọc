#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        string a;
        int i = 0;
        cin >> a;
        while (i < a.length())
        {
            int s = 0, pos = i;
            while (a[pos] == a[i])
            {
                pos++;
                s++;
            }
            cout << s << a[i];
            i = pos;
        }
        cout << "\n";
    }
}
