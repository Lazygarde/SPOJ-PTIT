#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int a, b, x = 0, y = 0, z = 0;
	cin >> a >> b;
	for (int i = 1; i <= 6; i++)
	{
		if (abs(i - a) < abs(i - b))
			x++;
		else if (abs(i - a) > abs(i - b))
			z++;
		else
			y++;
	}
	cout << x << ' ' << y << ' ' << z;
}
