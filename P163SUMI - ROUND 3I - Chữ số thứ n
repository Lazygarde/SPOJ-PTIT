#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int p = 1;
	string a = "";
	while (a.size() <= 1000)
	{
		string b = "";
		int pos = p;
		while (pos != 0)
		{
			char k = pos % 10 + '0';
			b = k + b;
			pos /= 10;
		}
		a += b;
		p++;
	}
	cout << a[n - 1];
}
