#include <bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int n;
		cin>>n;
		for(int i=1;i<=(n-1)/9;i++) cout<<"123456790";
		int t=(n-1)%9;
		for(int i=1;i<=t;i++) cout<<i;
		for(int i=t+1;i>=2;i--) cout<<i;
		for(int i=1;i<=(n-1)/9;i++) cout<<"098765432";
		cout<<"1"<<endl;
	}
}
