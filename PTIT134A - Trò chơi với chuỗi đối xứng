#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    string a;
    while (cin >> a)
    {
        int s = 0, b[26] = {};
        for (int i = 0; i < a.size(); i++)
            b[a[i] - 'a']++;
        for (int i = 0; i < 26; i++)
            if (b[i] % 2 == 1)
                s++;
        if (s % 2 == 1 || s == 0)
            cout << "First\n";
        else
            cout << "Second\n";
    }
}
