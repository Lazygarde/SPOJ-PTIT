#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string a, b;
	int s = 0;
	cin >> a >> b;
	if (a.size() > b.size())
		swap(a, b);
	int k = a.size(), h = b.size();
	for (int i = 1; i <= k; i++)
	{
		if (k % i == 0 && h % i == 0)
		{
			int ok = 1;
			string y = a.substr(0, i);
			for (int j = i; j < k; j += i)
			{
				string x = a.substr(j, i);
				if (y != x)
				{
					ok = 0;
					break;
				}
			}
			if (ok == 1)
			{
				for (int j = 0; j < h; j += i)
				{
					string x = b.substr(j, i);
					if (y != x)
					{
						ok = 0;
						break;
					}
				}
			}
			if (ok == 1)
				s++;
		}
	}
	cout << s;
}
