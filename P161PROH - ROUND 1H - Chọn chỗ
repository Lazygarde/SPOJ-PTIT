#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int a[n], b[5000] = {};
	for (int i = 0; i < n; i++)
		cin >> a[i];
	for (int i = 0; i < n; i++)
		b[a[i]]++;
	for (int i = 1; i < 5000; i++)
	{
		if (b[i] == 0)
		{
			cout << i;
			return 0;
		}
	}
}
