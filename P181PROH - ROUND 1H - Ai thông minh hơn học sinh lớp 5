#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int a, b;
	cin >> a >> b;
	if (b == a)
		cout << "infinity";
	else if (a > b)
	{
		int k = a - b, s = 0;
		for (int i = 1; i < sqrt(k); i++)
		{
			if (k % i == 0)
			{
				if (i > b)
					s++;
				if (k / i > b)
					s++;
			}
		}
		int j = sqrt(k);
		if (j * j == k && k % j == 0 && j > b)
			s++;
		cout << s;
	}
	else
		cout << "0";
}
