#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
vector<int> a(1000001, 0);
void era()
{
	for (long long i = 2; i * i <= 1000000; i++)
		if (!a[i])
			for (long long j = i * i; j <= 1000000; j += i)
				a[j] = 1;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	era();
	int t;
	cin >> t;
	while (t--)
	{
		int b, c, s = 0;
		cin >> b >> c;
		for (int i = b; i <= c; i++)
		{
			if (!a[i] && !a[i + 6] && i + 6 <= c)
				s++;
		}
		cout << s << "\n";
	}
}
