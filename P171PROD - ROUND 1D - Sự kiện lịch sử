#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
struct data
{
	int l;
	int r;
};
bool cmp(data a, data b)
{
	if (a.l == b.l)
		return a.r < b.r;
	return a.l < b.l;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, k = 0, ans = 0;
	cin >> n;
	vector<data> a(n);
	for (int i = 0; i < n; i++)
		cin >> a[i].l >> a[i].r;
	sort(a.begin(), a.end(), cmp);
	for (int i = 0; i < n; i++)
	{
		if (i == 0 || a[i].r > a[k].r)
			k = i;
		if (a[i].l > a[k].l && a[i].r < a[k].r)
			ans++;
	}
	cout << ans;
}
