#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string a;
	cin >> a;
	int n;
	cin >> n;
	int b[a.size() + 1][26] = {};
	for (int i = 1; i <= a.size(); i++)
	{
		for (int j = 0; j < 26; j++)
			b[i][j] += b[i - 1][j];
		b[i][a[i - 1] - 'a']++;
	}
	for (int i = 0; i < n; i++)
	{
		int e, f, g, h, ok = 1;
		cin >> e >> f >> g >> h;
		for (int j = 0; j < 26; j++)
		{
			if ((b[f][j] - b[e - 1][j]) != (b[h][j] - b[g - 1][j]))
			{
				ok = 0;
				break;
			}
		}
		if (ok == 0)
			cout << "NO\n";
		else
			cout << "YES\n";
	}
}
