#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    vector<string> s;
    while (1)
    {
        string ss;
        cin >> ss;
        if (ss == "#")
            break;
        int y, n, a, p;
        y = n = a = p = 0;
        for (int i = 0; i < ss.size(); i++)
        {
            if (ss[i] == 'Y')
                y++;
            if (ss[i] == 'N')
                n++;
            if (ss[i] == 'A')
                a++;
            if (ss[i] == 'P')
                p++;
        }
        if (a >= (ss.size() + 1) / 2)
            s.push_back("need quorum");
        else if (y > n)
            s.push_back("yes");
        else if (y < n)
            s.push_back("no");
        else
            s.push_back("tie");
    }
    for (int i = 0; i < s.size(); i++)
        cout << s[i] << endl;
}
