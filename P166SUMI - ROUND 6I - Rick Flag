#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int s = 0;
	int n, x;
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		cin >> x;
		if (x == 0)
			s++;
	}
	if (s == 1)
		cout << "YES";
	else
		cout << "NO";
}
