#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
struct sv
{
    string s;
    int b;
    int c;
    int d;
};
bool cmp(sv e, sv f)
{
    if (e.d == f.d)
    {
        if (e.c == f.c)
            return e.b > f.b;
        return e.c > f.c;
    }
    return e.d > f.d;
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n;
    cin >> n;
    cin.ignore();
    sv a[n];
    for (int i = 0; i < n; i++)
        cin >> a[i].s >> a[i].b >> a[i].c >> a[i].d;
    sort(a, a + n, cmp);
    cout << a[0].s << endl << a[n - 1].s;
}
