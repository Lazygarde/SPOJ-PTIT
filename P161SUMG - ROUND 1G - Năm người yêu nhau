#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int a[5], s = 0;
	for (int i = 0; i < 5; i++)
	{
		cin >> a[i];
		s += a[i];
	}
	if (s % 5 == 0 && s != 0)
		cout << s / 5;
	else
		cout << "-1";
}
