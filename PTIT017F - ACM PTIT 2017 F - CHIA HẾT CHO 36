#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		string a;
		cin >> a;
		int s = 0;
		int n = a.size();
		for (int i = 0; i < a.size(); i++)
			s += a[i] - '0';
		if (a.size() == 1)
		{
			if (a == "0")
				cout << "YES\n";
			else
				cout << "NO\n";
		}
		else
		{
			int ss = (a[n - 2] - '0') * 10 + a[n - 1] - '0';
			if (ss % 4 == 0 && s % 9 == 0)
				cout << "YES\n";
			else
				cout << "NO\n";
		}
	}
}
