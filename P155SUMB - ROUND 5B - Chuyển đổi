#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string s;
	cin >> s;
	string a = "0000" + s, b = s + "0000", c;
	int n = a.size() - 1, nn = 0;
	for (int i = n; i >= 0; i--)
	{
		int k = (a[i] - '0') + (b[i] - '0') + nn;
		if (k >= 2)
		{
			nn = 1;
			c = (char)(k - 2 + '0') + c;
		}
		else
		{
			nn = 0;
			c = (char)(k + '0') + c;
		}
	}
	if (nn == 1)
		c = '1' + c;
	cout << c;
}
