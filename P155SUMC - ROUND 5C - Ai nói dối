#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int k;
	cin >> k;
	string a, b;
	getline(cin, a);
	getline(cin, b);
	int m = a.size(), n = b.size();
	int p = abs(m - n);
	if (p >= k)
		cout << "NO";
	else
		cout << "YES";
}
