#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    long long n, k, s = 0, ss = 0, pos = 0;
    cin >> n >> k;
    long long a[n + 2];
    a[0] = 0;
    for (int i = 1; i <= n; i++)
    {
        cin >> a[i];
        s += a[i];
    }
    sort(a, a + n + 1);
    for (int i = 1; i <= n; i++)
    {
        ss += (a[i] - a[i - 1]) * (n + 1 - i);
        pos++;
        if ((s - ss) < k)
            break;
    }
    ss -= (a[pos] - a[pos - 1]) * (n + 1 - pos);
    cout << a[pos - 1] + (s - ss - k) / (n + 1 - pos);
}
