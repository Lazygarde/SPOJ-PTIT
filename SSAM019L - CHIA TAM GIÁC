#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        double n, k;
        cin >> n >> k;
        for (int i = 1; i < n; i++)
        {
            double h = sqrt(i / n) * k;
            cout << fixed << setprecision(6) << h << " ";
        }
        cout << "\n";
    }
}
