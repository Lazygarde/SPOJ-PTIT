#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, k, s = 0;
	cin >> n >> k;
	int a[n], b[n];
	map<long long, long long> c, d;
	vector<int> e, f;
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
		c[a[i]]++;
	}
	for (int i = 0; i < n; i++)
	{
		cin >> b[i];
		d[b[i]]++;
	}
	for (int i = 0; i < n; i++)
		if (c[a[i]] != d[a[i]])
			e.push_back(a[i]);
	for (int i = 0; i < n; i++)
		if (c[b[i]] != d[b[i]])
			f.push_back(b[i]);
	for (int i = 0; i < e.size(); i++)
	{
		for (int j = 0; j < f.size(); j++)
		{
			if (e[i] == f[j])
			{
				e.erase(e.begin() + i, e.begin() + i + 1);
				f.erase(f.begin() + i, f.begin() + i + 1);
			}
		}
	}
	if (e.size() > 1)
		cout << "NO";
	else if (e.size() == 0)
		cout << "YES";
	else
	{
		if (abs(e[0] - f[0]) > k)
			cout << "NO";
		else
			cout << "YES";
	}
}
