#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, s = 0;
    cin >> n;
    int a[n + 3], x = n;
    for (int i = 1; i <= n; i++)
        cin >> a[i];
    for (int i = n; i >= 1; i--)
    {
        if (a[i] == x)
            x--;
        else
            s++;
    }
    cout << s;
}
