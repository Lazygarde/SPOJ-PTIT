#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int nt(long long k)
{
	if (k < 2)
		return 0;
	for (int i = 2; i <= sqrt(k); i++)
		if (k % i == 0)
			return 0;
	return 1;
}
int gnt(long long n)
{
	long long k = sqrt(n);
	if (k * k == n)
		if (nt(k) == 1)
			return 1;
	return 0;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long t;
	cin >> t;
	while (t--)
	{
		long long n;
		cin >> n;
		if (gnt(n) == 1)
			cout << "YES\n";
		else
			cout << "NO\n";
	}
}
