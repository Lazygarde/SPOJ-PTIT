#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	while (n--)
	{
		string a;
		cin >> a;
		if (a[a.size() - 1] == '6' && a[a.size() - 2] == '8')
			cout << "1" << endl;
		else
			cout << "0" << endl;
	}
}
