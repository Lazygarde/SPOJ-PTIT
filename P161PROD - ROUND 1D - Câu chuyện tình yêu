#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	char k;
	string a;
	cin >> k >> a;
	char a0[3][10] = {'q', 'w', 'e', 'r', 't', 'y', 'u', 'i', 'o', 'p', 'a', 's', 'd', 'f', 'g', 'h', 'j', 'k', 'l', ';', 'z', 'x', 'c', 'v', 'b', 'n', 'm', ',', '.', '/'};
	if (k == 'L')
	{
		for (int i = 0; i < a.size(); i++)
		{
			int ok = 1;
			for (int i1 = 0; i1 < 3; i1++)
			{
				for (int i2 = 0; i2 < 10; i2++)
				{
					if (a[i] == a0[i1][i2])
					{
						if (i2 == 9)
							cout << a0[i1][0];
						else
							cout << a0[i1][i2 + 1];
						ok = 0;
						break;
					}
				}
				if (ok == 0)
					break;
			}
		}
	}
	else
	{
		for (int i = 0; i < a.size(); i++)
		{
			int ok = 1;
			for (int i1 = 0; i1 < 3; i1++)
			{
				for (int i2 = 0; i2 < 10; i2++)
				{
					if (a[i] == a0[i1][i2])
					{
						if (i2 == 0)
							cout << a0[i1][9];
						else
							cout << a0[i1][i2 - 1];
						ok = 0;
						break;
					}
				}
				if (ok == 0)
					break;
			}
		}
	}
}
