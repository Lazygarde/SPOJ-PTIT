#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, t;
	cin >> n >> t;
	int a[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	sort(a, a + n);
	while (t--)
	{
		int k, s = 0;
		cin >> k;
		int i = 0, j = n - 1;
		while (i <= j)
		{
			int m = (i + j) / 2;
			if (a[m] <= k)
			{
				s = m + 1;
				i = m + 1;
			}
			else
				j = m - 1;
		}
		cout << s << endl;
	}
}
