#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int a[n];
	vector<int> b;
	for (int i = 0; i < n; i++)
		cin >> a[i];
	sort(a, a + n);
	b.push_back(a[0]);
	for (int i = 1; i < n; i++)
		if (a[i] != a[i - 1])
			b.push_back(a[i]);
	int s = b.size();
	if (s == 1 || s == 2)
		cout << "YES";
	else if (s > 3)
		cout << "NO";
	else
	{
		if (b[0] + b[2] == 2 * b[1])
			cout << "YES";
		else
			cout << "NO";
	}
}
