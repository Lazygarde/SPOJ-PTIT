#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string a;
	cin >> a;
	long long s = 0, ss = 0;
	for (int i = 0; i < a.size(); i++)
	{
		if (a[i] == '1')
			ss++;
		else if (a[i] == '2')
			s += ss;
	}
	cout << s;
}
