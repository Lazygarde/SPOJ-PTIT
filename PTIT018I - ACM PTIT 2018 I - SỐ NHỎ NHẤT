#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		long long x, y, z, n;
		cin >> x >> y >> z >> n;
		long long m = (x * y) / __gcd(x, y);
		m = m * z / __gcd(m, z);
		if (ceil(pow(10, n - 1) / m) < ceil(pow(10, n) / m))
			cout << m * (long long)(ceil(pow(10, n - 1) / m)) << endl;
		else
			cout << "-1" << endl;
	}
}
