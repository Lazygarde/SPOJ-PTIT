#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	long long a[n], b[n], s1 = 0, s2 = 0;
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
		s1 = s1 | a[i];
	}
	for (int i = 0; i < n; i++)
	{
		cin >> b[i];
		s2 = s2 | b[i];
	}
	cout << s1 + s2;
}
