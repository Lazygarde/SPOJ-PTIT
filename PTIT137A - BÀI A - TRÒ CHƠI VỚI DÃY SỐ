#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    while (1)
    {
        int a[22];
        for (int i = 1; i <= 4; i++)
            cin >> a[i];
        if (a[1] == 0 && a[2] == 0 && a[3] == 0 && a[4] == 0)
            break;
        int ok = 1, d = 0;
        for (int k = 1; k < 4; k++)
        {
            if (a[k] != a[1])
            {
                ok = 0;
                break;
            }
        }
        if (ok == 0)
        {
            int kt1 = 0;
            for (int i = 1; i <= 1000; i++)
            {
                d = i;
                int tg = a[1];
                for (int k = 1; k <= 4; k++)
                {
                    if (k != 4)
                    {
                        if (a[k] > a[k + 1])
                            a[k] = a[k] - a[k + 1];
                        else
                            a[k] = a[k + 1] - a[k];
                    }
                    else
                    {
                        if (a[k] > tg)
                            a[k] = a[k] - tg;
                        else
                            a[k] = tg - a[k];
                    }
                }
                int kt2 = 1;
                for (int k = 1; k <= 4; k++)
                {
                    if (a[k] != a[1])
                    {
                        kt2 = 0;
                        break;
                    }
                }
                if (kt2 == 1)
                {
                    kt1 = 1;
                    break;
                }
            }
            if (kt1 == 1)
                cout << d << endl;
        }
        else
            cout << "0" << endl;
    }
}
