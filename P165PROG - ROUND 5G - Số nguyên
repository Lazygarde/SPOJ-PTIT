#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string a;
	cin >> a;
	string a1 = "9223372036854775808";
	string a2 = "2147483647";
	if (a[0] == '-')
		a.erase(0, 1);
	if (a.size() > 19)
		cout << "BigInteger";
	else if (a.size() == 19 && a > a1)
		cout << "BigInteger";
	else if (a.size() > 10)
		cout << "long";
	else if (a.size() == 10 && a > a2)
		cout << "long";
	else if (a.size() > 5)
		cout << "int";
	else if (a.size() == 5 && a > "32767")
		cout << "int";
	else if (a.size() > 3)
		cout << "short";
	else if (a.size() == 3 && a > "127")
		cout << "short";
	else
		cout << "byte";
}
