#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
vector<int> a(3004, 0);
vector<int> b(3004, 0);
void era()
{
	for (int i = 2; i <= 3000; i++)
	{
		if (a[i] == 0)
		{
			for (int j = i; j <= 3000; j += i)
			{
				a[j] = 1;
				b[j]++;
			}
		}
	}
	for (int i = 0; i < 3000; i++)
	{
		if (b[i] == 2)
			b[i] = 1;
		else
			b[i] = 0;
	}
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, s = 0;
	cin >> n;
	era();
	for (int i = 0; i <= n; i++)
	{
		if (b[i] == 1)
			s++;
	}
	cout << s;
}
