#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n, j = 1;
		cin >> n;
		long long s = 0;
		int a[10];
		a[0] = 1;
		for (int i = 1; i < 10; i++)
			a[i] = a[i - 1] * i;
		while (n != 0)
		{
			int k = n % 10;
			s += k * a[j];
			j++;
			n /= 10;
		}
		cout << s << endl;
	}
}
