#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, t, l, r;
    cin >> n >> t;
    vector<int> a(n);
    for (int i = 0; i < n; i++)
        cin >> a[i];
    sort(a.begin(), a.end());
    while (t--)
    {
        cin >> l >> r;
        int k1 = lower_bound(a.begin(), a.end(), l) - a.begin();
        int k2 = upper_bound(a.begin(), a.end(), r) - a.begin();
        cout << k2 - k1 << endl;
    }
}
