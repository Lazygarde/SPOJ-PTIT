#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		long long x, ok = 1;
		cin >> x;
		long long n = 180 / __gcd(x, (long long)180);
		long c = x / __gcd(x, (long long)180);
		if (n - c >= 2)
		{
			if (n >= 3 && n <= 998244353)
				cout << n;
			else
				cout << "-1";
		}
		else
		{
			if (2 * n >= 3 && 2 * n <= 998244353)
				cout << 2 * n;
			else
				cout << "-1";
		}
		cout << endl;
	}
}
