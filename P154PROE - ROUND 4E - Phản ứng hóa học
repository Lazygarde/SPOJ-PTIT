#include <bits/stdc++.h>
using namespace std;
int n, m, x, y, s;
vector<vector<int>> a(51 + 1);
vector<bool> check(51, 0);
void dfs(int u)
{
    for (int i = 0; i < a[u].size(); i++)
    {
        int v = a[u][i];
        if (check[v] == 0)
        {
            check[v] = 1;
            s++;
            dfs(v);
        }
    }
}
int main()
{
    cin >> n >> m;
    for (int i = 0; i < m; i++)
    {
        cin >> x >> y;
        a[x].push_back(y);
        a[y].push_back(x);
    }
    long long ans = 1;
    for (int i = 1; i <= n; i++)
    {
        check.resize(51, 0);
        if (!check[i])
        {
            check[i] = 1;
            s = 0;
            dfs(i);
            for (int i = 0; i < s; i++)
                ans *= 2;
        }
    }
    cout << ans;
}
