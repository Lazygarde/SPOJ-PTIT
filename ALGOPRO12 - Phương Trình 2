#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, s = 1;
	cin >> n;
	for (int i = 2; i <= sqrt(n); i++)
	{
		if (n % i == 0)
		{
			int k = 0;
			while (n % i == 0)
			{
				n /= i;
				k++;
			}
			s *= 2 * k + 1;
		}
	}
	if (n > 1)
		s *= 3;
	cout << s;
}
