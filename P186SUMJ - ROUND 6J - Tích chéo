#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, m, k, s = 0;
	cin >> n >> m >> k;
	map<long long, long long> e, f, g, h;
	long long a[n], b[m];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	for (int i = 0; i < m; i++)
	{
		cin >> b[i];
		if (b[i] > 0)
			g[b[i]]++;
		else
			h[(-1) * b[i]]++;
	}
	if (k == 0)
	{
		long long s1 = 0, s2 = 0;
		for (int i = 0; i < n; i++)
		{
			if (a[i] == 0)
				s1++;
		}
		for (int i = 0; i < m; i++)
		{
			if (b[i] == 0)
				s2++;
		}
		cout << s1 * m + s2 * n - s1 * s2;
	}
	else
	{
		for (int i = 0; i < n; i++)
		{
			if (a[i] == 0)
				continue;
			if (k % a[i] == 0)
			{
				long long x = k / a[i];
				if (x > 0)
				{
					if (g[x] > 0)
						s += g[x];
				}
				else
				{
					if (h[(-1) * x] > 0)
						s += h[(-1) * x];
				}
			}
		}
		cout << s;
	}
}
