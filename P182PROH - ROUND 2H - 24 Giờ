#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
string cong(string a, string b)
{
	int ok = 0;
	string s = "";
	if (a.size() > b.size())
		b.insert(0, a.size() - b.size(), '0');
	else if (a.size() < b.size())
		a.insert(0, b.size() - a.size(), '0');
	int temp = 0;
	for (int i = a.size() - 1; i >= 0; i--)
	{
		temp = a[i] + b[i] - 96 + temp;
		s.insert(0, 1, temp % 10 + 48);
		temp = temp / 10;
	}
	if (temp > 0)
	{
		s.insert(0, 1, temp + 48);
	}
	return s;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string a;
	cin >> a;
	if (a[8] == 'P')
	{
		if (a[0] == '1' && a[1] == '2')
		{
			cout << "12";
			for (int i = 2; i < 8; i++)
				cout << a[i];
		}
		else
		{
			string s = "";
			s = s + a[0];
			s = s + a[1];
			cout << cong(s, "12");
			for (int i = 2; i < 8; i++)
				cout << a[i];
		}
	}
	else
	{
		if (a[0] == '1' && a[1] == '2')
		{
			cout << "00";
			for (int i = 2; i < 8; i++)
				cout << a[i];
		}
		else
			for (int i = 0; i < 8; i++)
				cout << a[i];
	}
}
