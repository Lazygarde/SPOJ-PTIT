#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n, s = 0;
        cin >> n;
        int a[n];
        for (int i = 0; i < n; i++)
            cin >> a[i];
        for (int i = 0; i < n; i++)
        {
            int j = i, b = a[i], ss = 0;
            while (a[j] == b)
                j++;
            int c = a[j];
            while (1)
            {
                if (a[j] != c && a[j] != b)
                    break;
                else
                    j++;
            }
            for (int k = i; k < j; k++)
                if (a[k] == b)
                    ss++;
            if (ss > s)
                s = ss;
        }
        cout << s << endl;
    }
}
