#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, m;
    while (1)
    {
        cin >> m >> n;
        if (n == 0 && m == 0)
            return 0;
        double a[m + 1], ma = 0, k;
        for (int i = 1; i <= m; i++)
        {
            cin >> a[i];
            if (abs(a[i]) > ma)
                ma = abs(a[i]);
        }
        double s = ma;
        for (int i = 2; i <= n; i++)
        {
            ma = 0;
            for (int j = 1; j <= m; j++)
            {
                cin >> k;
                if (s != 0)
                    a[j] *= k / s;
                if (abs(a[j]) > ma)
                    ma = abs(a[j]);
            }
            s = ma;
        }
        int pos = 1;
        for (int i = 2; i <= m; i++)
        {
            if (a[i] >= a[pos])
                pos = i;
        }
        cout << pos << endl;
    }
}
