#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n, k;
		cin >> n;
		if (n % 4 == 0)
			k = n / 4;
		else
			k = n / 4 + 1;
		for (int i = 0; i < n - k; i++)
			cout << "9";
		for (int i = 0; i < k; i++)
			cout << "8";
		cout << endl;
	}
}
