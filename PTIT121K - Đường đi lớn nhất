#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	pair<int, string> a[n + 1][n + 1];
	for (int i = 0; i <= n; i++)
	{
		a[i][0].second = "";
		a[0][i].second = "";
	}
	for (int i = 1; i <= n; i++)
	{
		for (int j = 1; j <= n; j++)
		{
			cin >> a[i][j].first;
			char k = a[i][j].first + '0';
			a[i][j].second = max(a[i - 1][j].second, a[i][j - 1].second) + k;
		}
	}
	string x = a[n][n].second, z = "";
	while (x.size() % 4 != 0)
		x = "0" + x;
	n = x.size();
	for (int i = 0; i < n; i += 4)
	{
		char k;
		int c = 0;
		for (int j = i; j < i + 4; j++)
		{
			if (x[j] == '1')
				c += pow(2, 3 - j + i);
		}
		if (c > 9)
			k = 'A' + c - 10;
		else
			k = '0' + c;
		z += k;
	}
	while (z[0] == '0')
		z.erase(0, 1);
	if (z == "")
		cout << 0;
	else
		cout << z;
}
