#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, k = 0;
    cin >> n;
    string s;
    map<string, int> a;
    for (int i = 0; i < n; i++)
    {
        cin >> s;
        if (a[s] > i - a[s])
            k++;
        a[s]++;
    }
    cout << k;
}
