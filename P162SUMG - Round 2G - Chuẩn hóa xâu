#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios::sync_with_stdio(false);
	cin.tie(0);
	string s;
	cin >> s;
	int n = s.size(), d = 0;
	for (int i = 0; i < n; i++)
	{
		if (s[i] >= 'A' && s[i] <= 'Z')
			d++;
		else if (s[i] >= 'a' && s[i] <= 'z')
			d--;
	}
	if (d > 0)
	{
		for (int i = 0; i < n; i++)
			if (s[i] >= 'a' && s[i] <= 'z')
				s[i] = s[i] - 32;
	}
	else
	{
		for (int i = 0; i < n; i++)
			if (s[i] >= 'A' && s[i] <= 'Z')
				s[i] = s[i] + 32;
	}
	cout << s;
}
