#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    string a, b;
    cin >> a >> b;
    if (a.size() >= b.size())
    {
        cout << "No";
        return 0;
    }
    while (a.size() < b.size())
    {
        char k = b[0];
        b.erase(b.begin());
        if (k == 'B')
            for (int i = 0; i < b.size() / 2; i++)
                swap(b[i], b[b.size() - i - 1]);
    }
    if (a == b)
        cout << "Yes";
    else
        cout << "No";
}
