#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, x, y, z;
	cin >> n >> x >> y >> z;
	long long t = z;
	while (y != z)
	{
		if (y > z)
			y = y - z;
		else
			z = z - y;
	}
	if (t / y < n)
		cout << (t / y);
	else
		cout << n;
}
