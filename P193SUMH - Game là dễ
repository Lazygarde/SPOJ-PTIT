#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, m;
	cin >> n >> m;
	if (n >= m)
		cout << n - m;
	else
	{
		int dem = 0;

		while (n < m)
		{
			if (m % 2 == 1)
				m++;
			else
				m /= 2;
			dem++;
		}
		dem += n - m;
		cout << dem;
	}
}
