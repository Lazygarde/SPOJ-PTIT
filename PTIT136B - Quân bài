#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, m, c, d;
    cin >> n >> m;
    vector<string> a(n);
    for (int i = 0; i < n; i++)
        cin >> a[i];
    cin >> c >> d;
    for (int i = 0; i < n; i++)
        for (int j = m - 1; j >= 0; j--)
            a[i].push_back(a[i][j]);
    for (int i = n - 1; i >= 0; i--)
        a.push_back(a[i]);
    if (a[c - 1][d - 1] == '#')
        a[c - 1][d - 1] = '.';
    else
        a[c - 1][d - 1] = '#';
    for (int i = 0; i < a.size(); i++)
        cout << a[i] << endl;
}
