#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		long long n;
		cin >> n;
		long long s = n + 1;
		for (int i = 2; i <= sqrt(n); i++)
			if (n % i == 0)
				s += i + n / i;
		if (sqrt(n) == (float)sqrt(n))
			s -= sqrt(n);
		long long ss = s + 1;
		for (int i = 2; i <= sqrt(s); i++)
			if (s % i == 0)
				ss += i + s / i;
		if (sqrt(s) == (float)sqrt(s))
			ss -= sqrt(s);
		if (ss == 2 * n)
			cout << "YES\n";
		else
			cout << "NO\n";
	}
}
