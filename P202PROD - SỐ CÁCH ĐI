#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long a[51];
	a[1] = 1;
	a[2] = 2;
	a[3] = 4;
	a[4] = 8;
	for (int i = 5; i <= 50; i++)
		a[i] = a[i - 1] + a[i - 2] + a[i - 3] + a[i - 4];
	int t;
	cin >> t;
	while (t--)
	{
		int n;
		cin >> n;
		cout << a[n] << endl;
	}
}
