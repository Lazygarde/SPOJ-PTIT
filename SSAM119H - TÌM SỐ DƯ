#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        string a;
        cin >> a;
        int k = a.size(), b;
        if (k == 1)
            b = a[0] - 48;
        else
            b = a[k - 1] - 48 + 10 * (a[k - 2] - 48);
        if (b % 4 == 0)
            cout << "4\n";
        else
            cout << "0\n";
    }
}
