#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n, m;
        cin >> n >> m;
        cin.ignore();
        vector<string> a;
        string x;
        int b[5] = {};
        for (int i = 0; i < 5 * n + 1; i++)
        {
            cin >> x;
            a.push_back(x);
        }
        for (int i = 1; i < 5 * n + 1; i += 5)
        {
            for (int j = 1; j < 5 * m + 1; j += 5)
            {
                int k = i, s = 0;
                while (a[k][j] == '*')
                {
                    k++;
                    s++;
                }
                b[s]++;
            }
        }
        for (int i = 0; i < 5; i++)
            cout << b[i] << " ";
        cout << endl;
    }
}
