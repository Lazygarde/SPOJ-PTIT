#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
vector<int> b;
vector<int> a(1e4 + 2, 1);
void era()
{
	for (long long i = 2; i <= 10000; i++)
	{
		if (a[i] == 1)
		{
			for (long long j = i * i; j <= 10000; j += i)
				a[j] = 0;
			b.push_back(i);
		}
	}
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	era();
	int t;
	cin >> t;
	while (t--)
	{
		int n;
		cin >> n;
		cout << b[n - 1] << endl;
	}
}
