#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, s = 0;
	cin >> n;
	for (int i = 1; i <= n; i++)
	{
		for (int j = i + 1; j < n; j++)
		{
			int k = i * i + j * j;
			int h = sqrt(k);
			if (h > n)
				break;
			if (h * h == k)
				s++;
		}
	}
	cout << s;
}
