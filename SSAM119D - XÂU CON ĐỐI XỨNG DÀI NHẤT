#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
string a;
bool check(int x, int y)
{
    for (int i = x; i <= (x + y) / 2; i++)
    {
        if (a[i] != a[y - i + x])
            return 0;
    }
    return 1;
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        cin >> a;
        int n = a.size(), s = 1;
        for (int i = 0; i < n; i++)
        {
            for (int j = n - 1; j > i; j--)
            {
                if (j - i + 1 > s && check(i, j))
                {
                    s = j - i + 1;
                    break;
                }
            }
        }
        cout << s << endl;
    }
}
