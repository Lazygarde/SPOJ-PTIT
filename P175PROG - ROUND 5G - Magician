#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
struct hero
{
	string id;
	string name;
	char wl;
};
bool cmp(hero e, hero f)
{
	if (e.id.size() == f.id.size())
		return e.id > f.id;
	else
		return e.id.size() > f.id.size();
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	vector<hero> a(n);
	vector<hero> b;
	for (int i = 0; i < n; i++)
		cin >> a[i].id >> a[i].name >> a[i].wl;
	for (int i = 0; i < n; i++)
	{
		if (a[i].name == "AM" && a[i].wl == 'W')
			b.push_back(a[i]);
	}
	sort(b.begin(), b.end(), cmp);
	cout << b[b.size() - 1].id;
}
