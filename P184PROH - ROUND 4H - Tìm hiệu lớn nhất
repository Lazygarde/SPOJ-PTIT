#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
long long xuly(long long a[], int i, int j)
{
	if (i >= j)
		return -1e18;
	if (i == j - 1)
		return a[j] - a[i];
	int vtmin = i, vtmax = i;
	for (int k = i + 1; k <= j; k++)
	{
		if (a[k] > a[vtmax])
			vtmax = k;
		if (a[k] < a[vtmin])
			vtmin = k;
	}
	if (vtmin < vtmax)
		return a[vtmax] - a[vtmin];
	return max(max(xuly(a, i, vtmax), xuly(a, vtmax + 1, vtmin - 1)), xuly(a, vtmin, j));
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	long long max1 = -1e18, min1 = 1e18;
	int xmax = 0, ymin = 0;
	long long a[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	cout << xuly(a, 0, n - 1);
}
