#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, s = 0;
	cin >> n;
	for (int i = 1; i < 10000; i++)
	{
		s += i * (i + 1) / 2;
		if (s > n)
		{
			cout << i - 1;
			return 0;
		}
	}
}
