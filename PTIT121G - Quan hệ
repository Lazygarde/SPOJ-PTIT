#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	while (1)
	{
		int n, ok = 1;
		cin >> n;
		if (n == 0)
			return 0;
		vector<int> a1;
		vector<int> a2;
		int a[n + 1][n + 1];
		for (int i = 1; i <= n; i++)
			for (int j = 1; j <= n; j++)
				cin >> a[i][j];
		for (int i = 1; i <= n; i++)
		{
			int ok1 = 0;
			for (int j = 0; j < a1.size(); j++)
			{
				if (a[i][a1[j]] == 1)
				{
					ok1 = 1;
					break;
				}
			}
			int ok2 = 0;
			for (int j = 0; j < a2.size(); j++)
			{
				if (a[i][a2[j]] == 1)
				{
					ok2 = 1;
					break;
				}
			}
			if (ok1 == 1 && ok2 == 0)
				a2.push_back(i);
			else if (ok1 == 0 && ok2 == 1)
				a1.push_back(i);
			else if (ok1 == 1 && ok2 == 1)
				ok = 0;
			else
				a1.push_back(i);
		}
		if (ok == 1)
			cout << "YES" << endl;
		else
			cout << "NO" << endl;
	}
}
