#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, m, a, b;
    cin >> n >> m >> a >> b;
    vector<string> c(n);
    vector<string> d;
    for (int i = 0; i < n; i++)
        cin >> c[i];
    for (int i = 0; i < n; i++)
    {
        string s = "";
        for (int j = 0; j < m; j++)
            s.insert(s.size(), b, c[i][j]);
        for (int j = 0; j < a; j++)
            cout << s << endl;
    }
}
