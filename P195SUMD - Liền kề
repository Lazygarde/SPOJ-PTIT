#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, s = 0;
	cin >> n;
	n *= 2;
	int a[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	for (int i = 0; i < n; i += 2)
	{
		if (a[i] == a[i + 1])
			continue;
		int j = i + 2;
		while (a[j] != a[i])
			j++;
		while (j != i + 1)
		{
			swap(a[j], a[j - 1]);
			s++;
			j--;
		}
	}
	cout << s;
}
