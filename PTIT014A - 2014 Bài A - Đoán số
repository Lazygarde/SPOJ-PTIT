#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	cin.ignore();
	while (t--)
	{
		string n;
		getline(cin, n);
		if (n == "dung")
			cout << "2" << endl;
		else if (n == "lon hon")
			cout << "1" << endl;
		else
			cout << "3" << endl;
	}
}
