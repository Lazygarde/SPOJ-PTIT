#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, m, h, u, v;
	cin >> n >> m >> h;
	vector <vector <int> > a(n);
	vector <int> dd(n, 1e9);
	queue < pair<int, int>> q;
	for (int i = 0;i < m;i++) {
		cin >> u;
		dd[u] = 0;
		q.push({ u, 0 });
	}
	for (int i = 0;i < h;i++) {
		cin >> u >> v;
		a[u].push_back(v);
		a[v].push_back(u);
	}
	while (q.size()) {
		u = q.front().first;
		int s = q.front().second;
		q.pop();
		for (auto v : a[u]) {
			if (dd[v] == 1e9) {
				dd[v] = s + 1;
				q.push({ v, s + 1 });
			}
		}
	}
	int x = 0, p;
	for (int i = 0;i < n;i++) {
		if (dd[i] > x) {
			x = dd[i];
			p = i;
		}
	}
	cout << p;
}
