#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
vector<int> a(1e7 + 2, 1);
void era()
{
	a[1] = 0;
	for (long long i = 2; i <= 5000000; i++)
	{
		for (long long j = i * 2; j <= 10000000; j += i)
		{
			a[j] += i;
		}
	}
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	era();
	int c, b;
	cin >> c >> b;
	long long s = 0;
	for (int i = c; i <= b; i++)
		s += abs(i - a[i]);
	cout << s;
}
