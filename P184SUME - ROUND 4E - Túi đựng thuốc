#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int m, n, mod = 1e9 + 7;
	cin >> m >> n;
	int f[m + 1] = {}, a[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	f[0] = 1;
	for (int i = 0; i < n; i++)
		for (int j = a[i]; j <= m; j++)
			f[j] = (f[j] + f[j - a[i]]) % mod;
	cout << f[m];
}
