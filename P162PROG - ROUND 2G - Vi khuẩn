#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, k, s = 0;
	cin >> n;
	while (n > 0)
	{
		s++;
		k = 1;
		while (2 * k <= n)
			k *= 2;
		n = n - k;
	}
	cout << s;
}
