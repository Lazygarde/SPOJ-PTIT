#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, k;
	cin >> n >> k;
	vector<string> a(n);
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	sort(a.begin(), a.end());
	long long m = sqrt(k);
	while (m * (m + 1) / 2 < k)
		m++;
	while (m * (m + 1) / 2 > k)
		m--;
	k -= m * (m + 1) / 2;
	cout << a[k - 1];
}
