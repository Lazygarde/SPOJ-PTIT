#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		string a;
		cin >> a;
		if (next_permutation(a.begin(), a.end()))
			cout << a << endl;
		else
			cout << "BIGGEST\n";
	}
}
