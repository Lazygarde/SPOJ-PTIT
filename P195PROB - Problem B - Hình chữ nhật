#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, s = 0;
	cin >> n;
	int a[n][3], b[101][101] = {};
	for (int i = 0; i < n; i++)
	{
		cin >> a[i][0] >> a[i][1] >> a[i][2];
		for (int j1 = a[i][0] + 1; j1 <= a[i][1]; j1++)
			for (int j2 = 1; j2 <= a[i][2]; j2++)
				b[j1][j2]++;
	}
	for (int i = 0; i <= 100; i++)
		for (int j = 0; j <= 100; j++)
			if (b[i][j] > 0)
				s++;
	cout << s;
}
