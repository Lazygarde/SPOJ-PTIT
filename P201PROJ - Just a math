#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		long long a, b;
		cin >> a >> b;
		long long k = b;
		long long s = -1, ss = 1;
		while (b != 0)
		{
			s++;
			ss *= 10;
			b /= 10;
		}
		if (k == ss - 1)
			s++;
		cout << a * s << endl;
	}
}
