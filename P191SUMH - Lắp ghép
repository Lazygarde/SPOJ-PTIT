#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int a[n + 2][n + 2];
	char x;
	for (int i = 0; i <= n + 1; i++)
		for (int j = 0; j <= n + 1; j++)
			a[i][j] = 1;
	for (int i = 1; i <= n; i++)
	{
		for (int j = 1; j <= n; j++)
		{
			cin >> x;
			if (x == '#')
				a[i][j] = 1;
			else
				a[i][j] = 0;
		}
	}
	for (int i = 1; i <= n; i++)
	{
		for (int j = 1; j <= n; j++)
		{
			if (a[i][j] == 0)
			{
				if (a[i + 1][j] == 0 && a[i + 1][j + 1] == 0 && a[i + 1][j - 1] == 0 && a[i + 2][j] == 0)
				{
					a[i + 1][j] = 1;
					a[i + 1][j + 1] = 1;
					a[i + 1][j - 1] = 1;
					a[i + 2][j] = 1;
				}
				else
				{
					cout << "NO";
					return 0;
				}
			}
		}
	}
	cout << "YES";
}
