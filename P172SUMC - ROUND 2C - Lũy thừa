#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n;
	cin >> n;
	if (n == 0)
		cout << "1";
	else if (n % 4 == 1)
		cout << "8";
	else if (n % 4 == 2)
		cout << "4";
	else if (n % 4 == 3)
		cout << "2";
	else if (n % 4 == 0)
		cout << "6";
}
