#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	if (n > 0)
		cout << n;
	else
	{
		if (n == -10)
			cout << "0";
		else
		{
			n *= -1;
			int a = n / 10, b = n % 10, c = n / 100;
			c = c * 10 + b;
			if (a > c)
				cout << "-" << c;
			else
				cout << "-" << a;
		}
	}
}
