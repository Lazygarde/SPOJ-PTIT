#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
long long Try(long long a)
{
	long long c = 0;
	while (a != 0)
	{
		if (a % 10 != 0)
			c = c * 10 + a % 10;
		a /= 10;
	}
	while (c != 0)
	{
		a = a * 10 + c % 10;
		c /= 10;
	}
	return a;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long a, b;
	cin >> a >> b;
	long long h = a + b;
	h = Try(h);
	a = Try(a);
	b = Try(b);
	if (a + b == h)
		cout << "YES";
	else
		cout << "NO";
}
