#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int x, d = 0, ok = 1;
        cin >> x;
        for (int i = 1; i <= x; i++)
            if (__gcd(i, x) == 1)
                d++;
        for (int i = 2; i <= sqrt(d); i++)
        {
            if (d % i == 0)
            {
                ok = 0;
                break;
            }
        }
        if (ok == 0 || d == 0 || d == 1)
            cout << "0\n";
        else
            cout << "1\n";
    }
}
