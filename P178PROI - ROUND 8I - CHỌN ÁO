#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
struct b
{
	int l;
	int r;
};
bool cmp(b e, b f)
{
	return e.l < f.l;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n, m, s = 0, j = 0;
		cin >> n;
		int a[n];
		for (int i = 0; i < n; i++)
			cin >> a[i];
		sort(a, a + n);
		cin >> m;
		b c[m];
		for (int i = 0; i < m; i++)
			cin >> c[i].l >> c[i].r;
		sort(c, c + m, cmp);
		for (int i = 0; i < n; i++)
		{
			while (1)
			{
				if (a[i] < c[j].l)
				{
					if (j > 1)
						j -= 2;
					break;
				}
				if (c[j].l <= a[i] && c[j].r >= a[i])
				{
					s++;
					break;
				}
				else
					j++;
			}
		}
		cout << s << endl;
	}
}
