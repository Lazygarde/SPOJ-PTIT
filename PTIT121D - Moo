#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n;
	cin >> n;
	long long a[29], k = 27;
	a[1] = 3;
	for (int i = 2; i <= 28; i++)
		a[i] = 2 * a[i - 1] + i + 2;
	while (1)
	{
		if (n == 1)
		{
			cout << "m";
			return 0;
		}
		else if (n == 2 || n == 3)
		{
			cout << "o";
			return 0;
		}
		if (n > a[k] && n <= a[k] + k + 3)
		{
			if (n == a[k] + 1)
				cout << "m";
			else
				cout << "o";
			return 0;
		}
		else if (n >= a[k] + k + 4)
			n -= a[k] + k + 3;
		k--;
	}
}
