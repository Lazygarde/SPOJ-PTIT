#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    long long n, s1 = 0, s2 = 0;
    cin >> n;
    long long m = n;
    for (int i = 2; i <= sqrt(n); i++)
    {
        int k = i, s3 = 0;
        if (n % i == 0)
        {
            while (k != 0)
            {
                s3 += k % 10;
                k /= 10;
            }
        }
        while (n % i == 0)
        {
            s1 += s3;
            n /= i;
        }
    }
    if (n > 1)
    {
        while (n != 0)
        {
            s1 += n % 10;
            n /= 10;
        }
    }
    while (m != 0)
    {
        s2 += m % 10;
        m /= 10;
    }
    if (s1 == s2)
        cout << "YES";
    else
        cout << "NO";
}
