#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
long long c(int n, int k)
{
	long long ans = 1;
	for (int i = 1; i <= k; i++, n--)
		ans = ans * n / i;
	return ans;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	while (1)
	{
		int n;
		cin >> n;
		if (n == 0)
			break;
		cout << c(2 * n, n) / (n + 1) << endl;
	}
}
