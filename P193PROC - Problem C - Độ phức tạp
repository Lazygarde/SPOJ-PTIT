#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	string s;
	int a[26] = {}, ss = 0, sss = 0;
	cin >> s;
	int n = s.size();
	for (int i = 0; i < n; i++)
		a[s[i] - 'a']++;
	sort(a, a + 26);
	for (int i = 0; i < 26; i++)
	{
		if (a[i] > 0)
			ss++;
	}
	for (int i = 26 - ss; i < 26 - 2; i++)
		sss += a[i];
	cout << sss;
}
