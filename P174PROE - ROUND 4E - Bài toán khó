#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int a[n];
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	vector<int> c;
	c.push_back(a[0]);
	for (int i = 0; i < n; i++)
	{
		int s = 0;
		for (int j = 0; j < c.size(); j++)
		{
			if (a[i] != c[j])
				s++;
		}
		if (s == c.size())
			c.push_back(a[i]);
	}
	if (c.size() == 1 || c.size() == 2)
		cout << "YES";
	else if (c.size() == 3)
	{
		sort(c.begin(), c.end());
		if (c[0] + c[2] == 2 * c[1])
			cout << "YES";
		else
			cout << "NO";
	}
	else
		cout << "NO";
}
