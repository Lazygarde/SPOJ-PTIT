#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n;
		cin >> n;
		string s = "";
		for (int i = 1; i <= n; i++)
		{
			stringstream ss;
			ss << i;
			s += ss.str();
		}
		int temp = 2, dem = 0;
		vector<int> a(s.length());
		for (int i = 0; i < a.size(); i++)
			a[i] = i;
		while (a.size() > 1)
			if (temp == 2)
			{
				a.resize((a.size() + 1) / 2);
				int x = 0;
				for (int i = 0; i < a.size(); i++)
				{
					a[i] += x;
					x += pow(2, dem);
				}
				temp = 1;
				dem++;
			}
			else
			{
				a.resize(a.size() / 2);
				int x = pow(2, dem);
				for (int i = 0; i < a.size(); i++)
				{
					a[i] += x;
					x += pow(2, dem);
				}
				temp = 2;
				dem++;
			}
		cout << s[a[0]] << endl;
	}
}
