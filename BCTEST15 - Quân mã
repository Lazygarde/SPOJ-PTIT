#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
long long max(long long a, long long b, long long c, long long d)
{
    long long x = a;
    x = max(x, b);
    x = max(x, c);
    x = max(x, d);
    return x;
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, m;
    cin >> n >> m;
    long long a[n + 5][m + 5] = {}, ans = 0;
    for (int i = 2; i <= n + 1; i++)
    {
        for (int j = 2; j <= m + 1; j++)
        {
            cin >> a[i][j];
            a[i][j] = a[i][j] + max(a[i - 1][j - 2], a[i - 1][j + 2], a[i - 2][j - 1], a[i - 2][j + 1]);
            if (i == n + 1)
                ans = max(ans, a[i][j]);
        }
    }
    cout << ans;
}
