#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, k, ss = 0;
	string a;
	cin >> n >> a;
	a = '0' + a;
	for (int i = 1; i <= n; i++)
	{
		ss += a[i] - 48;
		if (ss == 3)
		{
			k = i;
			break;
		}
	}
	while (1)
	{
		int s = 0, ok = 1;
		for (int i = 0; i < k; i++)
			s += a[i] - '0';
		for (int i = 1; i <= n - k + 1; i++)
		{
			s = s + a[i + k - 1] - a[i - 1];
			if (s < 3)
			{
				k++;
				ok = 0;
				break;
			}
		}
		if (ok == 1)
		{
			cout << k;
			return 0;
		}
	}
}
