#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, s;
    cin >> n;
    for (int i = 0; i <= n / 3; i++)
    {
        float k = (float)(n - 3 * i) / 5;
        if (k == (int)k)
        {
            s = (int)k + i;
            cout << s;
            return 0;
        }
    }
    cout << "-1";
}
