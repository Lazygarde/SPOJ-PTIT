#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    long long n, k;
    cin >> n >> k;
    long long a[n];
    for (int i = 0; i < n; i++)
        cin >> a[i];
    sort(a, a + n);
    vector<long long> l(k + 1, 1e18);
    l[0] = 0;
    for (int i = 1; i <= k; i++)
        for (int j = 0; j < n; j++)
            if (a[j] <= i && l[i - a[j]] + 1 < l[i])
                l[i] = l[i - a[j]] + 1;
    if (l[k] != 1e18)
        cout << l[k];
    else
        cout << -1;
}
