#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n, k = 1;
		cin >> n;
		for (int i = 2 * n - 1; i > n; i--)
		{
			if ((long long)i * n % (i - n) == 0)
			{
				k += 2;
			}
		}
		cout << k << endl;
	}
}
