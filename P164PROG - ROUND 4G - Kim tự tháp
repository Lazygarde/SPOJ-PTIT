#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int s = 0, t = 0, bs = 0;
	while (s < n)
	{
		t++;
		bs = bs + t;
		s = s + bs;
	}
	if (s == n)
		cout << t;
	else if (s > n)
		cout << (t - 1);
}
