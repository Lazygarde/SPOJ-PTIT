#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int a[n];
	for (int j = 0; j < n; j++)
	{
		string s;
		cin >> s;
		vector<int> l(26, 0);
		for (int i = 0; i < s.length(); i++)
			l[s[i] - 97]++;
		int dem = 0;
		for (int i = 0; i < 26; i++)
			if (l[i] % 2 == 1)
				dem++;
		if (dem <= 3)
			a[j] = 1;
		else
			a[j] = 0;
	}
	for (int i = 0; i < n; i++)
		if (a[i])
			cout << "YES" << endl;
		else
			cout << "NO" << endl;
}
