#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	double a[6], s = 0;
	for (int i = 0; i < 6; i++)
		cin >> a[i];
	double b[3];
	for (int i = 0; i < 6; i += 2)
	{
		b[i / 2] = sqrt(a[i] * a[i] + a[i + 1] * a[i + 1] + a[i] * a[i + 1]);
		s += 0.5 * a[i] * a[i + 1] * 0.5 * 1.732050808;
	}
	double k = (b[0] + b[1] + b[2]) / 2;
	s += sqrt(k * (k - b[0]) * (k - b[1]) * (k - b[2]));
	long long h = 4 * s / 1.732050808 + 1;
	cout << h;
}
