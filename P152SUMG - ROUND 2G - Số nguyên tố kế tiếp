#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
bool nto(int n)
{
    for (int i = 2; i <= sqrt(n); i++)
        if (n % i == 0)
            return 0;
    return 1;
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int n, m, ok = 1;
    cin >> n >> m;
    for (int i = n + 1; i < m; i++)
    {
        if (nto(i))
        {
            ok = 0;
            break;
        }
    }
    if (ok == 1 && nto(n == 1) && nto(m) == 1 && m != n && m > n)
        cout << "YES";
    else
        cout << "NO";
}
