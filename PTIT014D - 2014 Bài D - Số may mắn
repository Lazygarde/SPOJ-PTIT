#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	cin.ignore();
	while (t--)
	{
		string n;
		cin >> n;
		while (n.size() > 1)
		{
			int s = 0;
			for (int i = 0; i < n.size(); i++)
				s += n[i] - 48;
			n = "";
			while (s != 0)
			{
				char h = s % 10 + 48;
				s /= 10;
				n = h + n;
			}
		}
		if (n == "9")
			cout << "1" << endl;
		else
			cout << "0" << endl;
	}
}
