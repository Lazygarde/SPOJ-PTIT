#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int a[n], b[n], s = 0;
	for (int i = 0; i < n; i++)
		cin >> a[i] >> b[i];
	for (int i = 0; i < n; i++)
	{
		int ok = 0;
		for (int j = 0; j < n; j++)
			if (i != j && b[j] == a[i])
				ok = 1;
		if (ok == 0)
			s++;
	}
	cout << s;
}
