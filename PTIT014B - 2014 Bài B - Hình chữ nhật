#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		long xa, ya, ua, va, xb, yb, ub, vb;
		cin >> xa >> ya >> ua >> va >> xb >> yb >> ub >> vb;
		if ((vb >= ya && vb <= va && xb <= ua && ub >= xa) || (vb > va && yb <= va && xb <= ua && ub >= xa))
			cout << "1" << endl;
		else
			cout << "0" << endl;
	}
}
