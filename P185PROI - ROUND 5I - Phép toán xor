#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
string np(long long n)
{
	if (n == 1)
		return "1";
	return np(n / 2) + static_cast<char>(n % 2 + 48);
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		long long n;
		cin >> n;
		string s = np(n);
		long long tong = 0;
		for (int i = 0; i < s.length(); i++)
			if (s[i] == '0')
				tong += pow(2, (s.length() - i - 1));
		cout << tong << endl;
	}
}
