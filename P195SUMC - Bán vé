#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n, ok = 1;
	cin >> n;
	int a[n], b[2] = {};
	for (int i = 0; i < n; i++)
		cin >> a[i];
	for (int i = 0; i < n; i++)
	{
		if (a[i] == 25)
			b[0]++;
		else if (a[i] == 50)
		{
			if (b[0] == 0)
			{
				ok = 0;
				break;
			}
			else
			{
				b[1]++;
				b[0]--;
			}
		}
		else
		{
			if (b[1] > 1 && b[0] > 1)
			{
				b[0]--;
				b[1]--;
			}
			else if (b[0] > 3)
				b[0] -= 3;
			else
			{
				ok = 0;
				break;
			}
		}
	}
	if (ok == 1)
		cout << "YES";
	else
		cout << "NO";
}
