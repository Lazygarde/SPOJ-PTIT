#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin >> n;
    int a[n + 1], s2 = 0, s3 = -1e9, b[n + 1], s1 = -1e9;
    b[0] = 0;
    for (int i = 1; i <= n; i++)
    {
        cin >> a[i];
        if (a[i] > 0)
        {
            s2 += a[i];
        }
        s3 = max(s3, a[i]);
        b[i] = -1e9;
    }
    for (int i = 1; i <= n; i++)
    {
        b[i] = max(a[i], b[i - 1] + a[i]);
        s1 = max(s1, b[i]);
    }
    if (s2 == 0)
    {
        s2 = s3;
    }
    cout << s1 << " " << s2;
}
