#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	int sd = 0, am = -1e9, x, s = 0, k = 0, k1 = 0, k2 = 0;
	for (int i = 1; i <= n; i++)
	{
		cin >> x;
		k += x;
		if (k < 0)
			k = 0;
		else
			s = max(s, k);
		if (x >= 0)
		{
			sd += x;
			k1 = 1;
		}
		else
		{
			if (x > am)
				am = x;
			k2 = 1;
		}
	}
	if (k1 == 0)
		cout << am << " " << am;
	else if (k2 == 0)
		cout << sd << " " << sd;
	else
		cout << s << " " << sd;
}
