#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    while (1)
    {
        long long a, b, c;
        cin >> a >> b >> c;
        if (a == 0 && b == 0 && c == 0)
            return 0;
        if (a * a + b * b == c * c || c * c + b * b == a * a || c * c + a * a == b * b)
            cout << "right\n";
        else
            cout << "wrong\n";
    }
}
