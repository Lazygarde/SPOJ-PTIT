#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n;
		cin >> n;
		int dem = 0;
		for (int i = 0; i <= sqrt(n / 2); i++)
		{
			int a = n - i * i;
			if (sqrt(a) == round(sqrt(a)))
				dem++;
		}
		cout << dem << endl;
	}
}
