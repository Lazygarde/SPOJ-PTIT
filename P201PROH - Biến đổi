#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int a, b;
		cin >> a >> b;
		if (a == 3)
		{
			if (b > 3)
				cout << "NO\n";
			else
				cout << "YES\n";
		}
		else if (a == 2)
		{
			if (b > 3)
				cout << "NO\n";
			else
				cout << "YES\n";
		}
		else if (a == 1)
		{
			if (b > 1)
				cout << "NO\n";
			else
				cout << "YES\n";
		}
		else
			cout << "YES\n";
	}
}
