#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		int n, ok = 1;
		cin >> n;
		int a[n], b[n];
		for (int i = 0; i < n; i++)
			cin >> a[i];
		for (int i = 0; i < n; i++)
			cin >> b[i];
		sort(a, a + n);
		sort(b, b + n);
		for (int i = 0; i < n; i++)
		{
			if (b[i] > a[i])
			{
				ok = 0;
				break;
			}
		}
		if (ok == 0)
		{
			for (int i = 0; i < n; i++)
			{
				if (a[i] > b[i])
				{
					ok = 1;
					break;
				}
			}
			if (ok == 1)
				cout << "NO\n";
			else
				cout << "YES\n";
		}
		else
			cout << "YES\n";
	}
}
