#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		string a;
		cin >> a;
		string s1 = a.substr(0, a.size() / 2);
		string s2 = a.substr(a.size() / 2, a.size());
		int s3 = 0, s4 = 0;
		for (int i = 0; i < s1.size(); i++)
			s3 += s1[i] - 'A';
		for (int i = 0; i < s1.size(); i++)
		{
			int k = (s1[i] - 'A' + s3) % 26;
			char h = k + 'A';
			s1[i] = h;
		}
		for (int i = 0; i < s2.size(); i++)
			s4 += s2[i] - 'A';
		for (int i = 0; i < s2.size(); i++)
		{
			int k = (s2[i] - 'A' + s4) % 26;
			char h = k + 'A';
			s2[i] = h;
		}
		for (int i = 0; i < s1.size(); i++)
		{
			int k = (s1[i] - 'A' + s2[i] - 'A') % 26;
			k = k % 26;
			char h = k + 'A';
			s1[i] = h;
		}
		cout << s1 << endl;
	}
}
