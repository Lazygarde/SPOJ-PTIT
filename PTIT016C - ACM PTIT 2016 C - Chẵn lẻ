#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	cin.ignore();
	while (t--)
	{
		string s;
		cin >> s;
		int a[s.size()], ok = 1;
		for (int i = 0; i < s.size(); i++)
			a[i] = s[i] - 48;
		for (int i = 0; i < s.size(); i++)
		{
			if (i % 2 == 0 && a[i] % 2 == 0)
			{
				ok = 0;
				break;
			}
			else if (i % 2 == 1 && a[i] % 2 == 1)
			{
				ok = 0;
				break;
			}
		}
		if (ok == 0)
			cout << "NO" << endl;
		else
			cout << "YES" << endl;
	}
}
