#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        string a;
        cin >> a;
        int ok = 1;
        for (int i = a.size() - 1; i > 0; i--)
        {
            if (ok == 0)
            {
                if (a[i] > '3')
                {
                    a[i] = '0';
                    ok = 0;
                }
                else
                {
                    a[i] = '0';
                    ok = 1;
                }
            }
            else
            {
                if (a[i] > '4')
                {
                    a[i] = '0';
                    ok = 0;
                }
                else
                {
                    a[i] = '0';
                    ok = 1;
                }
            }
        }
        if (ok == 0)
        {
            if (a[0] == '9')
            {
                a[0] = '0';
                a = "1" + a;
            }
            else
                a[0]++;
        }
        cout << a << endl;
    }
}
