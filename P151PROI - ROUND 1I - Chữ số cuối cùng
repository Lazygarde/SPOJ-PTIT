#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n;
        cin >> n;
        while (n > 9)
        {
            int k = n, s = 0;
            while (k != 0)
            {
                s += k % 10;
                k /= 10;
            }
            n = s;
        }
        cout << n << "\n";
    }
}
