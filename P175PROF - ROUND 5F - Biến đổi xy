#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		string a;
		cin >> a;
		int s1 = 0, s2 = 0;
		for (int i = 0; i < a.size(); i++)
		{
			if (a[i] == 'x')
				s1++;
			else
				s2++;
		}
		if (s1 > s2)
			for (int i = 0; i < s1 - s2; i++)
				cout << 'x';
		else
			for (int i = 0; i < s2 - s1; i++)
				cout << 'y';
		cout << endl;
	}
}
