#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n;
	cin >> n;
	long long a[n], s = 0, ma = 0, maa = 0;
	for (int i = 0; i < n; i++)
		cin >> a[i];
	for (int i = 0; i < n; i++)
	{
		s += a[i];
		if (s < 0)
			s = 0;
		if (s > ma)
			ma = s;
	}
	s = 0;
	for (int i = 0; i < n; i++)
	{
		s += a[i];
		if (s > 0)
			s = 0;
		if (s < maa)
			maa = s;
	}
	if (abs(maa) > ma)
		cout << abs(maa);
	else
		cout << ma;
}
