#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	long long a[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	sort(a, a + n);
	if (n % 2 == 1)
		cout << a[(n - 1) / 2];
	else
	{
		int k = n / 2;
		long long s1 = 0, s2 = 0;
		for (int i = 0; i < n; i++)
			s1 += abs(a[k - 1] - a[i]);
		for (int i = 0; i < n; i++)
			s2 += abs(a[k] - a[i]);
		if (s1 <= s2)
			cout << a[k - 1];
		else
			cout << a[k];
	}
}
