#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long n, a = 0, b = 1, s = 0;
	cin >> n;
	while (s < n)
	{
		long long c = a + b;
		b = a;
		a = c;
		s++;
	}
	cout << a;
}
