#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long a, b, s = 0;
	cin >> a >> b;
	while (a != 0 && b != 0)
	{
		s += (a / b);
		a %= b;
		swap(a, b);
	}
	cout << s;
}
