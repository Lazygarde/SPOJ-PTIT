#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long a[100], n, k = 5;
	a[1] = 1;
	for (int i = 2; i <= 30; i++)
	{
		a[i] = a[i - 1] + k;
		k *= 2;
	}
	cin >> n;
	for (int i = 2; i <= 30; i++)
	{
		if (a[i] > n)
		{
			n -= a[i - 1];
			cout << 5 * n / (a[i] - a[i - 1]) + 1;
			return 0;
		}
	}
}
