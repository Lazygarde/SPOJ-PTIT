#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
struct bai
{
	long long kho;
	long long tien;
};
struct sv
{
	long long gioi;
	long long pos;
	long long an;
};
bool cmp1(bai a, bai b)
{
	return a.kho < b.kho;
}
bool cmp2(sv a, sv b)
{
	return a.gioi < b.gioi;
}
bool cmp3(sv a, sv b)
{
	return a.pos < b.pos;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long s, b;
	cin >> s >> b;
	long long a[s];
	sv d[s];
	bai c[b];
	for (long long i = 0; i < s; i++)
	{
		cin >> a[i];
		d[i].gioi = a[i];
		d[i].pos = i;
	}
	for (long long i = 0; i < b; i++)
		cin >> c[i].kho >> c[i].tien;
	sort(c, c + b, cmp1);
	sort(d, d + s, cmp2);
	long long i = 0, pos1 = 0;
	long long x = 0;
	while (i < b)
	{
		while (c[i].kho <= d[pos1].gioi)
		{
			x += c[i].tien;
			i++;
			if (i == b)
				break;
		}
		d[pos1].an = x;
		pos1++;
		if (pos1 == s)
			break;
	}
	for (long long i = pos1; i < s; i++)
		d[i].an = x;
	sort(d, d + s, cmp3);
	for (long long i = 0; i < s; i++)
		cout << d[i].an << " ";
}
