#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	for (int j = 1; j <= t; j++)
	{
		int n;
		cin >> n;
		long long a[n], b[n];
		long long s = 0;
		for (int i = 0; i < n; i++)
			cin >> a[i];
		for (int i = 0; i < n; i++)
			cin >> b[i];
		sort(a, a + n);
		sort(b, b + n, greater<int>());
		for (int i = 0; i < n; i++)
			s += a[i] * b[i];
		cout << "Case #" << j << ": " << s << endl;
	}
}
