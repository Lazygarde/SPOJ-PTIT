#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	long long A, B;
    cin >> A >> B;
    if (A < B)
        swap(A, B);
    if (A == B)
    {
        cout << 1;
        return 0;
    }
    long long d = A - B;
    long long N = (B / d + 1) * d - B;
    long long maxx = (A + N) / __gcd(A + N, B + N) * (B + N);
    for(int i=2;i<=sqrt(d);i++)
    {
        if (d % i == 0)
        {
            long long N1 = (B / i + 1) * i - B;
            if (N1 < N)
            {
                long long a = (A + N1) / __gcd(A + N1, B + N1) * (B + N1);
                if (maxx >= a)
                {
                    maxx = a;
                    N = N1;
                }
            }
            long long b = d / i;
            long long N2 = (B / b + 1) * b - B;
            if (N2 < N)
            {
                long long a = (A + N2) / __gcd(A + N2, B + N2) * (B + N2);
                if (maxx >= a)
                {
                    maxx = a;
                    N = N2;
                }
            }
        }
    }
    cout << N;
}
