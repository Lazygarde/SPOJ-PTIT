#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
const long long p = 1e9 + 7;
long long a[201][201];
long long c(long long n, long long k)
{
	long long res = 1;
	for (long long i = 1; i <= k; i++, n--)
		res = res * n / i;
	return res;
}
void taomang()
{
	for (int i = 0; i <= 200; i++)
	{
		for (int j = 0; j <= i; j++)
		{
			if (j == 0 || j == i)
				a[i][j] = 1;
			else
				a[i][j] = (a[i - 1][j] % p + a[i - 1][j - 1] % p) % p;
		}
	}
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	long long a[n];
	map<long long, long long> b;
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
		b[a[i]]++;
	}
	sort(a, a + n);
	if (b[a[0]] >= 3)
		cout << c(b[a[0]], 3);
	else if (b[a[0]] + b[a[1]] >= 3)
	{
		if (b[a[0]] == 1)
			cout << 3 * c(b[a[1]], 2);
		else
			cout << 2 * 3 * (b[a[1]] + c(b[a[1]], 2));
	}
	else
		cout << 6 * b[a[0]] * b[a[1]] * b[a[2]];
}
