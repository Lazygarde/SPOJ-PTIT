#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        long long n, s = 0;
        cin >> n;
        long long k = sqrt(n);
        for (long long i = 1; i <= k; i++)
        {
            if (n % i == 0)
                s += n / i + i;
        }
        if (k * k == n)
            s -= k;
        cout << s << endl;
    }
}
