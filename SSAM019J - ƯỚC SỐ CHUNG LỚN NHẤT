#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n;
        cin >> n;
        long long a[n];
        for (int i = 0; i < n; i++)
            cin >> a[i];
        cout << a[0] << " ";
        for (int i = 1; i < n; i++)
            cout << a[i] * a[i - 1] / __gcd(a[i], a[i - 1]) << " ";
        cout << a[n - 1] << endl;
    }
}
