#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
struct data
{
	int c;
	int d;
};
bool cmp1(data a, data b)
{
	return a.c < b.c;
}
bool cmp2(data a, data b)
{
	return a.d < b.d;
}
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while (t--)
	{
		data a[4];
		for (int i = 0; i < 4; i++)
			cin >> a[i].c >> a[i].d;
		int ok = 0;
		sort(a, a + 4, cmp1);
		if (a[0].c == a[1].c && a[2].c == a[3].c && a[0].c != a[2].c)
			ok = 1;
		if (ok == 0)
			cout << "NO\n";
		else
		{
			sort(a, a + 4, cmp2);
			if (a[0].d == a[1].d && a[2].d == a[3].d && a[0].d != a[2].d)
				ok = 0;
			if (ok == 1)
				cout << "NO\n";
			else
				cout << "YES\n";
		}
	}
}
