#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	long long a[n], b[n + 1], s = 0, ma = 0, j = 1;
	for (int i = 0; i < n; i++)
		cin >> a[i];
	b[0] = 0;
	for (int i = n - 1; i >= 0; i--)
	{
		b[j] = a[i] + b[j - 1];
		j++;
	}
	j = 1;
	for (int i = 0; i < n; i++)
	{
		s += a[i];
		while (b[j] < s)
			j++;
		if (j + i >= n)
			break;
		if (b[j] == s)
			ma = s;
	}
	cout << ma;
}
