#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    long long n, k, x = n;
    cin >> n >> k;
    for (int i = 1; i <= k; i++)
    {
        if (n % i != i - 1)
        {
            cout << "No";
            return 0;
        }
    }
    cout << "Yes";
}
