#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin >> n;
	long long a[n];
	for (int i = 0; i < n; i++)
		cin >> a[i];
	int c = 0, d = n - 1, s = 0;
	while (c <= d)
	{
		if (a[c] == a[d])
		{
			c++;
			d--;
		}
		else if (a[c] > a[d])
		{
			a[d - 1] += a[d];
			d--;
			s++;
		}
		else
		{
			a[c + 1] += a[c];
			s++;
			c++;
		}
	}
	cout << s;
}
